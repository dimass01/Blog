[32m+[m
[32m+[m[32m  <xsd:simpleType name="parameter_type">[m
[32m+[m[32m    <xsd:restriction base="xsd:string">[m
[32m+[m[32m      <xsd:enumeration value="collection" />[m
[32m+[m[32m      <xsd:enumeration value="service" />[m
[32m+[m[32m      <xsd:enumeration value="string" />[m
[32m+[m[32m      <xsd:enumeration value="constant" />[m
[32m+[m[32m    </xsd:restriction>[m
[32m+[m[32m  </xsd:simpleType>[m
[32m+[m
[32m+[m[32m  <xsd:simpleType name="argument_type">[m
[32m+[m[32m    <xsd:restriction base="xsd:string">[m
[32m+[m[32m      <xsd:enumeration value="collection" />[m
[32m+[m[32m      <xsd:enumeration value="service" />[m
[32m+[m[32m      <xsd:enumeration value="string" />[m
[32m+[m[32m      <xsd:enumeration value="constant" />[m
[32m+[m[32m    </xsd:restriction>[m
[32m+[m[32m  </xsd:simpleType>[m
[32m+[m
[32m+[m[32m  <xsd:simpleType name="invalid_sequence">[m
[32m+[m[32m    <xsd:restriction base="xsd:string">[m
[32m+[m[32m      <xsd:enumeration value="null" />[m
[32m+[m[32m      <xsd:enumeration value="ignore" />[m
[32m+[m[32m      <xsd:enumeration value="exception" />[m
[32m+[m[32m    </xsd:restriction>[m
[32m+[m[32m  </xsd:simpleType>[m
[32m+[m
[32m+[m[32m  <xsd:simpleType name="boolean">[m
[32m+[m[32m    <xsd:restriction base="xsd:string">[m
[32m+[m[32m      <xsd:pattern value="(%.+%|true|false)" />[m
[32m+[m[32m    </xsd:restriction>[m
[32m+[m[32m  </xsd:simpleType>[m
[32m+[m[32m</xsd:schema>[m
