[32m+[m[32muse Symfony\Component\Debug\Debug;[m
[32m+[m
[32m+[m[32m$input = new ArgvInput();[m
[32m+[m[32m$env = $input->getParameterOption(array('--env', '-e'), getenv('SYMFONY_ENV') ?: 'dev');[m
[32m+[m[32m$debug = getenv('SYMFONY_DEBUG') !== '0' && !$input->hasParameterOption(array('--no-debug', '')) && $env !== 'prod';[m
[32m+[m
[32m+[m[32mif ($debug) {[m
[32m+[m[32m    Debug::enable();[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m$kernel = new AppKernel($env, $debug);[m
[32m+[m[32m$application = new Application($kernel);[m
[32m+[m[32m$application->run($input);[m
