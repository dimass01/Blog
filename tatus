[32m+[m[32m<?php[m
[32m+[m
[32m+[m[32mnamespace Sdz\Bundle\BlogBundle\Entity;[m
[32m+[m
[32m+[m[32muse Doctrine\ORM\EntityRepository;[m
[32m+[m[32muse Doctrine\ORM\Tools\Pagination\Paginator;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * ArticleRepository[m
[32m+[m[32m *[m
[32m+[m[32m * This class was generated by the Doctrine ORM. Add your own custom[m
[32m+[m[32m * repository methods below.[m
[32m+[m[32m */[m
[32m+[m[32mclass ArticleRepository extends EntityRepository[m
[32m+[m[32m{[m
[32m+[m[41m    [m
[32m+[m[41m    [m
[32m+[m[32m    public function testRequest()[m
[32m+[m[32m    {[m
[32m+[m[32m      // On utilise le QueryBuilder crÃ©Ã© par le repository directement pour gagner du temps[m
[32m+[m[32m      // Plus besoin de faire le select() ni le from() par la suite donc[m
[32m+[m
[32m+[m[32m      $qb = $this->_em->createQueryBuilder()[m
[32m+[m[32m             ->select("count(c.contenu)")[m
[32m+[m[32m                ->from("Sdz\Bundle\BlogBundle\Entity\Commentaire", 'c')[m
[32m+[m[32m                ->getQuery()[m
[32m+[m[32m                ->getSingleScalarResult();[m
[32m+[m[32m      return $qb;[m
[32m+[m[32m    }[m
[32m+[m[41m   [m
[32m+[m[32m    public function getArticles($nombreParPage, $page)[m
[32m+[m[32m    {[m
[32m+[m[41m        [m
[32m+[m[32m        // On dÃ©place la vÃ©rification du numÃ©ro de page dans cette mÃ©thode[m
[32m+[m[32m        if ($page < 1) {[m
[32m+[m[32m          throw new \InvalidArgumentException('L\'argument $page ne peut Ãªtre infÃ©rieur Ã  1 (valeur : "'.$page.'").');[m
[32m+[m[32m        }[m
[32m+[m[32m        $query = $this->createQueryBuilder('a')[m
[32m+[m[32m                      // On joint sur l'attribut image[m
[32m+[m[32m                      ->leftJoin('a.image', 'i')[m
[32m+[m[32m                        ->addSelect('i')[m
[32m+[m[32m                      // On joint sur l'attribut categories[m
[32m+[m[32m                      ->leftJoin('a.categories', 'c')[m
[32m+[m[32m                        ->addSelect('c')[m
[32m+[m[32m                      ->leftJoin('a.articleCompentences', 'aco')[m
[32m+[m[32m                        ->addSelect('aco')[m
[32m+[m[32m                      ->leftJoin('aco.competence', 'co')[m
[32m+[m[32m                        ->addSelect('co')[m
[32m+[m[32m                      ->orderBy('a.date', 'DESC')[m
[32m+[m[32m                      ->getQuery();[m
[32m+[m
[32m+[m[32m        // On dÃ©finit l'article Ã  partir duquel commencer la liste[m
[32m+[m[32m        $query->setFirstResult(($page-1) * $nombreParPage)[m
[32m+[m[32m        // Ainsi que le nombre d'articles Ã  afficher[m
[32m+[m[32m              ->setMaxResults($nombreParPage);[m
[32m+[m[32m           // Enfin, on retourne l'objet Paginator correspondant Ã  la requÃªte construite[m
[32m+[m[32m        // (n'oubliez pas le use correspondant en dÃ©but de fichier)[m
[32m+[m[32m        return new Paginator($query);[m[41m         [m
[32m+[m[32m    }[m
[32m+[m[41m [m
[32m+[m
[32m+[m
[32m+[m[32m    public function getAvecCategories(array $nom_categories)[m
[32m+[m[32m     {[m
[32m+[m[32m       $qb = $this->createQueryBuilder('a');[m
[32m+[m[32m       // On fait une jointure avec l'entitÃ© Categorie, avec pour alias Â« c Â»[m
[32m+[m[32m       $qb ->join('a.categories', 'c')[m
[32m+[m[32m           ->where($qb->expr()->in('c.nom', $nom_categories)); // Puis on filtre sur le nom des catÃ©gories Ã  l'aide d'un IN[m
[32m+[m[32m       // Enfin, on retourne le rÃ©sultat[m
[32m+[m[32m       return $qb->getQuery()[m
[32m+[m[32m                 ->getResult();[m
[32m+[m[32m     }[m
[32m+[m
[32m+[m[41m    [m
[32m+[m[41m   [m
[32m+[m[32m    public function getArticleAvecCommentaires()[m
[32m+[m[32m     {[m
[32m+[m[32m       $qb = $this->createQueryBuilder('a');[m
[32m+[m[32m       // On fait une jointure avec l'entitÃ© Categorie, avec pour alias Â« c Â»[m
[32m+[m[32m       $qb->addSelect('c')[m
[32m+[m[32m           ->leftJoin('a.commentaires', 'c');[m
[32m+[m[32m       // Enfin, on retourne le rÃ©sultat[m
[32m+[m[32m       return $qb->getQuery()[m
[32m+[m[32m                 ->getResult();[m
[32m+[m[32m     }[m
