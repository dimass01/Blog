[32m+[m[32m<?php[m
[32m+[m[32m// src/Sdz/BlogBundle/Antispam/SdzAntispam.php[m
[32m+[m[32mnamespace Sdz\Bundle\BlogBundle\Antispam;[m
[32m+[m[32mclass SdzAntispam[m
[32m+[m[32m{[m
[32m+[m[41m    [m
[32m+[m[41m    [m
[32m+[m[41m    [m
[32m+[m[32m    protected $mailer;[m
[32m+[m[32m    protected $locale;[m
[32m+[m[32m    protected $nbForSpam;[m
[32m+[m[32m    public function __construct(\Swift_Mailer $mailer, $locale, $nbForSpam)[m
[32m+[m[32m    {[m
[32m+[m[32m      $this->mailer    = $mailer;[m
[32m+[m[32m      $this->locale    = $locale;[m
[32m+[m[32m      $this->nbForSpam = (int) $nbForSpam;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[41m    [m
[32m+[m[32m  /**[m
[32m+[m[32m   * VÃ©rifie si le texte est un spam ou non[m
[32m+[m[32m   * Un texte est considÃ©rÃ© comme spam Ã  partir de 3 liens[m
[32m+[m[32m   * ou adresses e-mail dans son contenu[m
[32m+[m[32m   *[m
[32m+[m[32m   * @param string $text[m
[32m+[m[32m   */[m
[32m+[m[32m  public function isSpam($text)[m
[32m+[m[32m  {[m
[32m+[m[32m    return ($this->countLinks($text) + $this->countMails($text)) >= $this->nbForSpam;[m
[32m+[m[32m  }[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Compte les URL de $text[m
[32m+[m[32m   *[m
[32m+[m[32m   * @param string $text[m
[32m+[m[32m   */[m
[32m+[m[32m  private function countLinks($text)[m
[32m+[m[32m  {[m
[32m+[m[32m    preg_match_all([m
[32m+[m[32m      '#(http|https|ftp)://([A-Z0-9][A-Z0-9_-]*(?:.[A-Z0-9][A-Z0-9_-]*)+):?(d+)?/?#i',[m
[32m+[m[32m      $text,[m
[32m+[m[32m      $matches);[m
[32m+[m[32m    return count($matches[0]);[m
[32m+[m[32m  }[m
[32m+[m[32m  /**[m
[32m+[m[32m   * Compte les e-mails de $text[m
[32m+[m[32m   *[m
[32m+[m[32m   * @param string $text[m
[32m+[m[32m   */[m
[32m+[m[32m  private function countMails($text)[m
[32m+[m[32m  {[m
[32m+[m[32m    preg_match_all([m
[32m+[m[32m      '#[a-z0-9._-]+@[a-z0-9._-]{2,}\.[a-z]{2,4}#i',[m
[32m+[m[32m      $text,[m
[32m+[m[32m      $matches);[m
[32m+[m[32m    return count($matches[0]);[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
\ No newline at end of file[m
