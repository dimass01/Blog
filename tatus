[32m+[m
[32m+[m
[32m+[m[32m!function ($) {[m
[32m+[m
[32m+[m[32m  "use strict"; // jshint ;_;[m
[32m+[m
[32m+[m
[32m+[m[32m  /* CSS TRANSITION SUPPORT (http://www.modernizr.com/)[m
[32m+[m[32m   * ======================================================= */[m
[32m+[m
[32m+[m[32m  $(function () {[m
[32m+[m
[32m+[m[32m    $.support.transition = (function () {[m
[32m+[m
[32m+[m[32m      var transitionEnd = (function () {[m
[32m+[m
[32m+[m[32m        var el = document.createElement('bootstrap')[m
[32m+[m[32m          , transEndEventNames = {[m
[32m+[m[32m               'WebkitTransition' : 'webkitTransitionEnd'[m
[32m+[m[32m            ,  'MozTransition'    : 'transitionend'[m
[32m+[m[32m            ,  'OTransition'      : 'oTransitionEnd otransitionend'[m
[32m+[m[32m            ,  'transition'       : 'transitionend'[m
[32m+[m[32m            }[m
[32m+[m[32m          , name[m
[32m+[m
[32m+[m[32m        for (name in transEndEventNames){[m
[32m+[m[32m          if (el.style[name] !== undefined) {[m
[32m+[m[32m            return transEndEventNames[name][m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m      }())[m
[32m+[m
[32m+[m[32m      return transitionEnd && {[m
[32m+[m[32m        end: transitionEnd[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m    })()[m
[32m+[m
[32m+[m[32m  })[m
[32m+[m
[32m+[m[32m}(window.jQuery);/* ==========================================================[m
[32m+[m[32m * bootstrap-alert.js v2.3.2[m
[32m+[m[32m * http://getbootstrap.com/2.3.2/javascript.html#alerts[m
[32m+[m[32m * ==========================================================[m
[32m+[m[32m * Copyright 2013 Twitter, Inc.[m
[32m+[m[32m *[m
[32m+[m[32m * Licensed under the Apache License, Version 2.0 (the "License");[m
[32m+[m[32m * you may not use this file except in compliance with the License.[m
[32m+[m[32m * You may obtain a copy of the License at[m
[32m+[m[32m *[m
[32m+[m[32m * http://www.apache.org/licenses/LICENSE-2.0[m
[32m+[m[32m *[m
[32m+[m[32m * Unless required by applicable law or agreed to in writing, software[m
[32m+[m[32m * distributed under the License is distributed on an "AS IS" BASIS,[m
[32m+[m[32m * WITHOUT WARRANTIES OR CONDITIONS OF ANY KIND, either express or implied.[m
[32m+[m[32m * See the License for the specific language governing permissions and[m
[32m+[m[32m * limitations under the License.[m
[32m+[m[32m * ========================================================== */[m
[32m+[m
[32m+[m
[32m+[m[32m!function ($) {[m
[32m+[m
[32m+[m[32m  "use strict"; // jshint ;_;[m
[32m+[m
[32m+[m
[32m+[m[32m /* ALERT CLASS DEFINITION[m
[32m+[m[32m  * ====================== */[m
[32m+[m
[32m+[m[32m  var dismiss = '[data-dismiss="alert"]'[m
[32m+[m[32m    , Alert = function (el) {[m
[32m+[m[32m        $(el).on('click', dismiss, this.close)[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m  Alert.prototype.close = function (e) {[m
[32m+[m[32m    var $this = $(this)[m
[32m+[m[32m      , selector = $this.attr('data-target')[m
[32m+[m[32m      , $parent[m
[32m+[m
[32m+[m[32m    if (!selector) {[m
[32m+[m[32m      selector = $this.attr('href')[m
[32m+[m[32m      selector = selector && selector.replace(/.*(?=#[^\s]*$)/, '') //strip for ie7[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    $parent = $(selector)[m
[32m+[m
[32m+[m[32m    e && e.preventDefault()[m
[32m+[m
[32m+[m[32m    $parent.length || ($parent = $this.hasClass('alert') ? $this : $this.parent())[m
[32m+[m
[32m+[m[32m    $parent.trigger(e = $.Event('close'))[m
[32m+[m
[32m+[m[32m    if (e.isDefaultPrevented()) return[m
[32m+[m
[32m+[m[32m    $parent.removeClass('in')[m
[32m+[m
[32m+[m[32m    function removeElement() {[m
[32m+[m[32m      $parent[m
[32m+[m[32m        .trigger('closed')[m
[32m+[m[32m        .remove()[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    $.support.transition && $parent.hasClass('fade') ?[m
[32m+[m[32m      $parent.on($.support.transition.end, removeElement) :[m
[32m+[m[32m      removeElement()[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m
[32m+[m[32m /* ALERT PLUGIN DEFINITION[m
[32m+[m[32m  * ======================= */[m
[32m+[m
[32m+[m[32m  var old = $.fn.alert[m
[32m+[m
[32m+[m[32m  $.fn.alert = function (option) {[m
[32m+[m[32m    return this.each(function () {[m
[32m+[m[32m      var $this = $(this)[m
[32m+[m[32m        , data = $this.data('alert')[m
[32m+[m[32m      if (!data) $this.data('alert', (data = new Alert(this)))[m
[32m+[m[32m      if (typeof option == 'string') data[option].call($this)[m
[32m+[m[32m    })[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  $.fn.alert.Constructor = Alert[m
[32m+[m
[32m+[m
[32m+[m[32m /* ALERT NO CONFLICT[m
[32m+[m[32m  * ================= */[m
[32m+[m
[32m+[m[32m  $.fn.alert.noConflict = function () {[m
[32m+[m[32m    $.fn.alert = old[m
[32m+[m[32m    return this[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m
[32m+[m[32m /* ALERT DATA-API[m
[32m+[m[32m  * ============== */[m
[32m+[m
[32m+[m[32m  $(document).on('click.alert.data-api', dismiss, Alert.prototype.close)[m
[32m+[m
[32m+[m[32m}(window.jQuery);/* ============================================================[m
[32m+[m[32m * bootstrap-button.js v2.3.2[m
[32m+[m[32m * http://getbootstrap.com/2.3.2/javascript.html#buttons[m
[32m+[m[32m * ============================================================[m
[32m+[m[32m * Copyright 2013 Twitter, Inc.[m
[32m+[m[32m *[m
[32m+[m[32m * Licensed under the Apache License, Version 2.0 (the "License");[m
[32m+[m[32m * you may not use this file except in compliance with the License.[m
[32m+[m[32m * You may obtain a copy of the License at[m
[32m+[m[32m *[m
[32m+[m[32m * http://www.apache.org/licenses/LICENSE-2.0[m
[32m+[m[32m *[m
[32m+[m[32m * Unless required by applicable law or agreed to in writing, software[m
[32m+[m[32m * distributed under the License is distributed on an "AS IS" BASIS,[m
[32m+[m[32m * WITHOUT WARRANTIES OR CONDITIONS OF ANY KIND, either express or implied.[m
[32m+[m[32m * See the License for the specific language governing permissions and[m
[32m+[m[32m * limitations under the License.[m
[32m+[m[32m * ============================================================ */[m
[32m+[m
[32m+[m
[32m+[m[32m!function ($) {[m
[32m+[m
[32m+[m[32m  "use strict"; // jshint ;_;[m
[32m+[m
[32m+[m
[32m+[m[32m /* BUTTON PUBLIC CLASS DEFINITION[m
[32m+[m[32m  * ============================== */[m
[32m+[m
[32m+[m[32m  var Button = function (element, options) {[m
[32m+[m[32m    this.$element = $(element)[m
[32m+[m[32m    this.options = $.extend({}, $.fn.button.defaults, options)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  Button.prototype.setState = function (state) {[m
[32m+[m[32m    var d = 'disabled'[m
[32m+[m[32m      , $el = this.$element[m
[32m+[m[32m      , data = $el.data()[m
[32m+[m[32m      , val = $el.is('input') ? 'val' : 'html'[m
[32m+[m
[32m+[m[32m    state = state + 'Text'[m
[32m+[m[32m    data.resetText || $el.data('resetText', $el[val]())[m
[32m+[m
[32m+[m[32m    $el[val](data[state] || this.options[state])[m
[32m+[m
[32m+[m[32m    // push to event loop to allow forms to submit[m
[32m+[m[32m    setTimeout(function () {[m
[32m+[m[32m      state == 'loadingText' ?[m
[32m+[m[32m        $el.addClass(d).attr(d, d) :[m
[32m+[m[32m        $el.removeClass(d).removeAttr(d)[m
[32m+[m[32m    }, 0)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  Button.prototype.toggle = function () {[m
[32m+[m[32m    var $parent = this.$element.closest('[data-toggle="buttons-radio"]')[m
[32m+[m
[32m+[m[32m    $parent && $parent[m
[32m+[m[32m      .find('.active')[m
[32m+[m[32m      .removeClass('active')[m
[32m+[m
[32m+[m[32m    this.$element.toggleClass('active')[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m
[32m+[m[32m /* BUTTON PLUGIN DEFINITION[m
[32m+[m[32m  * ======================== */[m
[32m+[m
[32m+[m[32m  var old = $.fn.button[m
[32m+[m
[32m+[m[32m  $.fn.button = function (option) {[m
[32m+[m[32m    return this.each(function () {[m
[32m+[m[32m      var $this = $(this)[m
[32m+[m[32m        , data = $this.data('button')[m
[32m+[m[32m        , options = typeof option == 'object' && option[m
[32m+[m[32m      if (!data) $this.data('button', (data = new Button(this, options)))[m
[32m+[m[32m      if (option == 'toggle') data.toggle()[m
[32m+[m[32m      else if (option) data.setState(option)[m
[32m+[m[32m    })[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  $.fn.button.defaults = {[m
[32m+[m[32m    loadingText: 'loading...'[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  $.fn.button.Constructor = Button[m
[32m+[m
[32m+[m
[32m+[m[32m /* BUTTON NO CONFLICT[m
[32m+[m[32m  * ================== */[m
[32m+[m
[32m+[m[32m  $.fn.button.noConflict = function () {[m
[32m+[m[32m    $.fn.button = old[m
[32m+[m[32m    return this[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m
[32m+[m[32m /* BUTTON DATA-API[m
[32m+[m[32m  * =============== */[m
[32m+[m
[32m+[m[32m  $(document).on('click.button.data-api', '[data-toggle^=button]', function (e) {[m
[32m+[m[32m    var $btn = $(e.target)[m
[32m+[m[32m    if (!$btn.hasClass('btn')) $btn = $btn.closest('.btn')[m
[32m+[m[32m    $btn.button('toggle')[m
[32m+[m[32m  })[m
[32m+[m
[32m+[m[32m}(window.jQuery);/* ==========================================================[m
[32m+[m[32m * bootstrap-carousel.js v2.3.2[m
[32m+[m[32m * http://getbootstrap.com/2.3.2/javascript.html#carousel[m
[32m+[m[32m * ==========================================================[m
[32m+[m[32m * Copyright 2013 Twitter, Inc.[m
[32m+[m[32m *[m
[32m+[m[32m * Licensed under the Apache License, Version 2.0 (the "License");[m
[32m+[m[32m * you may not use this file except in compliance with the License.[m
[32m+[m[32m * You may obtain a copy of the License at[m
[32m+[m[32m *[m
[32m+[m[32m * http://www.apache.org/licenses/LICENSE-2.0[m
[32m+[m[32m *[m
[32m+[m[32m * Unless required by applicable law or agreed to in writing, software[m
[32m+[m[32m * distributed under the License is distributed on an "AS IS" BASIS,[m
[32m+[m[32m * WITHOUT WARRANTIES OR CONDITIONS OF ANY KIND, either express or implied.[m
[32m+[m[32m * See the License for the specific language governing permissions and[m
[32m+[m[32m * limitations under the License.[m
[32m+[m[32m * ========================================================== */[m
[32m+[m
[32m+[m
[32m+[m[32m!function ($) {[m
[32m+[m
[32m+[m[32m  "use strict"; // jshint ;_;[m
[32m+[m
[32m+[m
[32m+[m[32m /* CAROUSEL CLASS DEFINITION[m
[32m+[m[32m  * ========================= */[m
[32m+[m
[32m+[m[32m  var Carousel = function (element, options) {[m
[32m+[m[32m    this.$element = $(element)[m
[32m+[m[32m    this.$indicators = this.$element.find('.carousel-indicators')[m
[32m+[m[32m    this.options = options[m
[32m+[m[32m    this.options.pause == 'hover' && this.$element[m
[32m+[m[32m      .on('mouseenter', $.proxy(this.pause, this))[m
[32m+[m[32m      .on('mouseleave', $.proxy(this.cycle, this))[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  Carousel.prototype = {[m
[32m+[m
[32m+[m[32m    cycle: function (e) {[m
[32m+[m[32m      if (!e) this.paused = false[m
[32m+[m[32m      if (this.interval) clearInterval(this.interval);[m
[32m+[m[32m      this.options.interval[m
[32m+[m[32m        && !this.paused[m
[32m+[m[32m        && (this.interval = setInterval($.proxy(this.next, this), this.options.interval))[m
[32m+[m[32m      return this[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , getActiveIndex: function () {[m
[32m+[m[32m      this.$active = this.$element.find('.item.active')[m
[32m+[m[32m      this.$items = this.$active.parent().children()[m
[32m+[m[32m      return this.$items.index(this.$active)[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , to: function (pos) {[m
[32m+[m[32m      var activeIndex = this.getActiveIndex()[m
[32m+[m[32m        , that = this[m
[32m+[m
[32m+[m[32m      if (pos > (this.$items.length - 1) || pos < 0) return[m
[32m+[m
[32m+[m[32m      if (this.sliding) {[m
[32m+[m[32m        return this.$element.one('slid', function () {[m
[32m+[m[32m          that.to(pos)[m
[32m+[m[32m        })[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (activeIndex == pos) {[m
[32m+[m[32m        return this.pause().cycle()[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      return this.slide(pos > activeIndex ? 'next' : 'prev', $(this.$items[pos]))[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , pause: function (e) {[m
[32m+[m[32m      if (!e) this.paused = true[m
[32m+[m[32m      if (this.$element.find('.next, .prev').length && $.support.transition.end) {[m
[32m+[m[32m        this.$element.trigger($.support.transition.end)[m
[32m+[m[32m        this.cycle(true)[m
[32m+[m[32m      }[m
[32m+[m[32m      clearInterval(this.interval)[m
[32m+[m[32m      this.interval = null[m
[32m+[m[32m      return this[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , next: function () {[m
[32m+[m[32m      if (this.sliding) return[m
[32m+[m[32m      return this.slide('next')[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , prev: function () {[m
[32m+[m[32m      if (this.sliding) return[m
[32m+[m[32m      return this.slide('prev')[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , slide: function (type, next) {[m
[32m+[m[32m      var $active = this.$element.find('.item.active')[m
[32m+[m[32m        , $next = next || $active[type]()[m
[32m+[m[32m        , isCycling = this.interval[m
[32m+[m[32m        , direction = type == 'next' ? 'left' : 'right'[m
[32m+[m[32m        , fallback  = type == 'next' ? 'first' : 'last'[m
[32m+[m[32m        , that = this[m
[32m+[m[32m        , e[m
[32m+[m
[32m+[m[32m      this.sliding = true[m
[32m+[m
[32m+[m[32m      isCycling && this.pause()[m
[32m+[m
[32m+[m[32m      $next = $next.length ? $next : this.$element.find('.item')[fallback]()[m
[32m+[m
[32m+[m[32m      e = $.Event('slide', {[m
[32m+[m[32m        relatedTarget: $next[0][m
[32m+[m[32m      , direction: direction[m
[32m+[m[32m      })[m
[32m+[m
[32m+[m[32m      if ($next.hasClass('active')) return[m
[32m+[m
[32m+[m[32m      if (this.$indicators.length) {[m
[32m+[m[32m        this.$indicators.find('.active').removeClass('active')[m
[32m+[m[32m        this.$element.one('slid', function () {[m
[32m+[m[32m          var $nextIndicator = $(that.$indicators.children()[that.getActiveIndex()])[m
[32m+[m[32m          $nextIndicator && $nextIndicator.addClass('active')[m
[32m+[m[32m        })[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if ($.support.transition && this.$element.hasClass('slide')) {[m
[32m+[m[32m        this.$element.trigger(e)[m
[32m+[m[32m        if (e.isDefaultPrevented()) return[m
[32m+[m[32m        $next.addClass(type)[m
[32m+[m[32m        $next[0].offsetWidth // force reflow[m
[32m+[m[32m        $active.addClass(direction)[m
[32m+[m[32m        $next.addClass(direction)[m
[32m+[m[32m        this.$element.one($.support.transition.end, function () {[m
[32m+[m[32m          $next.removeClass([type, direction].join(' ')).addClass('active')[m
[32m+[m[32m          $active.removeClass(['active', direction].join(' '))[m
[32m+[m[32m          that.sliding = false[m
[32m+[m[32m          setTimeout(function () { that.$element.trigger('slid') }, 0)[m
[32m+[m[32m        })[m
[32m+[m[32m      } else {[m
[32m+[m[32m        this.$element.trigger(e)[m
[32m+[m[32m        if (e.isDefaultPrevented()) return[m
[32m+[m[32m        $active.removeClass('active')[m
[32m+[m[32m        $next.addClass('active')[m
[32m+[m[32m        this.sliding = false[m
[32m+[m[32m        this.$element.trigger('slid')[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      isCycling && this.cycle()[m
[32m+[m
[32m+[m[32m      return this[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m
[32m+[m[32m /* CAROUSEL PLUGIN DEFINITION[m
[32m+[m[32m  * ========================== */[m
[32m+[m
[32m+[m[32m  var old = $.fn.carousel[m
[32m+[m
[32m+[m[32m  $.fn.carousel = function (option) {[m
[32m+[m[32m    return this.each(function () {[m
[32m+[m[32m      var $this = $(this)[m
[32m+[m[32m        , data = $this.data('carousel')[m
[32m+[m[32m        , options = $.extend({}, $.fn.carousel.defaults, typeof option == 'object' && option)[m
[32m+[m[32m        , action = typeof option == 'string' ? option : options.slide[m
[32m+[m[32m      if (!data) $this.data('carousel', (data = new Carousel(this, options)))[m
[32m+[m[32m      if (typeof option == 'number') data.to(option)[m
[32m+[m[32m      else if (action) data[action]()[m
[32m+[m[32m      else if (options.interval) data.pause().cycle()[m
[32m+[m[32m    })[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  $.fn.carousel.defaults = {[m
[32m+[m[32m    interval: 5000[m
[32m+[m[32m  , pause: 'hover'[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  $.fn.carousel.Constructor = Carousel[m
[32m+[m
[32m+[m
[32m+[m[32m /* CAROUSEL NO CONFLICT[m
[32m+[m[32m  * ==================== */[m
[32m+[m
[32m+[m[32m  $.fn.carousel.noConflict = function () {[m
[32m+[m[32m    $.fn.carousel = old[m
[32m+[m[32m    return this[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m /* CAROUSEL DATA-API[m
[32m+[m[32m  * ================= */[m
[32m+[m
[32m+[m[32m  $(document).on('click.carousel.data-api', '[data-slide], [data-slide-to]', function (e) {[m
[32m+[m[32m    var $this = $(this), href[m
[32m+[m[32m      , $target = $($this.attr('data-target') || (href = $this.attr('href')) && href.replace(/.*(?=#[^\s]+$)/, '')) //strip for ie7[m
[32m+[m[32m      , options = $.extend({}, $target.data(), $this.data())[m
[32m+[m[32m      , slideIndex[m
[32m+[m
[32m+[m[32m    $target.carousel(options)[m
[32m+[m
[32m+[m[32m    if (slideIndex = $this.attr('data-slide-to')) {[m
[32m+[m[32m      $target.data('carousel').pause().to(slideIndex).cycle()[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    e.preventDefault()[m
[32m+[m[32m  })[m
[32m+[m
[32m+[m[32m}(window.jQuery);/* =============================================================[m
[32m+[m[32m * bootstrap-collapse.js v2.3.2[m
[32m+[m[32m * http://getbootstrap.com/2.3.2/javascript.html#collapse[m
[32m+[m[32m * =============================================================[m
[32m+[m[32m * Copyright 2013 Twitter, Inc.[m
[32m+[m[32m *[m
[32m+[m[32m * Licensed under the Apache License, Version 2.0 (the "License");[m
[32m+[m[32m * you may not use this file except in compliance with the License.[m
[32m+[m[32m * You may obtain a copy of the License at[m
[32m+[m[32m *[m
[32m+[m[32m * http://www.apache.org/licenses/LICENSE-2.0[m
[32m+[m[32m *[m
[32m+[m[32m * Unless required by applicable law or agreed to in writing, software[m
[32m+[m[32m * distributed under the License is distributed on an "AS IS" BASIS,[m
[32m+[m[32m * WITHOUT WARRANTIES OR CONDITIONS OF ANY KIND, either express or implied.[m
[32m+[m[32m * See the License for the specific language governing permissions and[m
[32m+[m[32m * limitations under the License.[m
[32m+[m[32m * ============================================================ */[m
[32m+[m
[32m+[m
[32m+[m[32m!function ($) {[m
[32m+[m
[32m+[m[32m  "use strict"; // jshint ;_;[m
[32m+[m
[32m+[m
[32m+[m[32m /* COLLAPSE PUBLIC CLASS DEFINITION[m
[32m+[m[32m  * ================================ */[m
[32m+[m
[32m+[m[32m  var Collapse = function (element, options) {[m
[32m+[m[32m    this.$element = $(element)[m
[32m+[m[32m    this.options = $.extend({}, $.fn.collapse.defaults, options)[m
[32m+[m
[32m+[m[32m    if (this.options.parent) {[m
[32m+[m[32m      this.$parent = $(this.options.parent)[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    this.options.toggle && this.toggle()[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  Collapse.prototype = {[m
[32m+[m
[32m+[m[32m    constructor: Collapse[m
[32m+[m
[32m+[m[32m  , dimension: function () {[m
[32m+[m[32m      var hasWidth = this.$element.hasClass('width')[m
[32m+[m[32m      return hasWidth ? 'width' : 'height'[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , show: function () {[m
[32m+[m[32m      var dimension[m
[32m+[m[32m        , scroll[m
[32m+[m[32m        , actives[m
[32m+[m[32m        , hasData[m
[32m+[m
[32m+[m[32m      if (this.transitioning || this.$element.hasClass('in')) return[m
[32m+[m
[32m+[m[32m      dimension = this.dimension()[m
[32m+[m[32m      scroll = $.camelCase(['scroll', dimension].join('-'))[m
[32m+[m[32m      actives = this.$parent && this.$parent.find('> .accordion-group > .in')[m
[32m+[m
[32m+[m[32m      if (actives && actives.length) {[m
[32m+[m[32m        hasData = actives.data('collapse')[m
[32m+[m[32m        if (hasData && hasData.transitioning) return[m
[32m+[m[32m        actives.collapse('hide')[m
[32m+[m[32m        hasData || actives.data('collapse', null)[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      this.$element[dimension](0)[m
[32m+[m[32m      this.transition('addClass', $.Event('show'), 'shown')[m
[32m+[m[32m      $.support.transition && this.$element[dimension](this.$element[0][scroll])[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , hide: function () {[m
[32m+[m[32m      var dimension[m
[32m+[m[32m      if (this.transitioning || !this.$element.hasClass('in')) return[m
[32m+[m[32m      dimension = this.dimension()[m
[32m+[m[32m      this.reset(this.$element[dimension]())[m
[32m+[m[32m      this.transition('removeClass', $.Event('hide'), 'hidden')[m
[32m+[m[32m      this.$element[dimension](0)[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , reset: function (size) {[m
[32m+[m[32m      var dimension = this.dimension()[m
[32m+[m
[32m+[m[32m      this.$element[m
[32m+[m[32m        .removeClass('collapse')[m
[32m+[m[32m        [dimension](size || 'auto')[m
[32m+[m[32m        [0].offsetWidth[m
[32m+[m
[32m+[m[32m      this.$element[size !== null ? 'addClass' : 'removeClass']('collapse')[m
[32m+[m
[32m+[m[32m      return this[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , transition: function (method, startEvent, completeEvent) {[m
[32m+[m[32m      var that = this[m
[32m+[m[32m        , complete = function () {[m
[32m+[m[32m            if (startEvent.type == 'show') that.reset()[m
[32m+[m[32m            that.transitioning = 0[m
[32m+[m[32m            that.$element.trigger(completeEvent)[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m      this.$element.trigger(startEvent)[m
[32m+[m
[32m+[m[32m      if (startEvent.isDefaultPrevented()) return[m
[32m+[m
[32m+[m[32m      this.transitioning = 1[m
[32m+[m
[32m+[m[32m      this.$element[method]('in')[m
[32m+[m
[32m+[m[32m      $.support.transition && this.$element.hasClass('collapse') ?[m
[32m+[m[32m        this.$element.one($.support.transition.end, complete) :[m
[32m+[m[32m        complete()[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , toggle: function () {[m
[32m+[m[32m      this[this.$element.hasClass('in') ? 'hide' : 'show']()[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m
[32m+[m[32m /* COLLAPSE PLUGIN DEFINITION[m
[32m+[m[32m  * ========================== */[m
[32m+[m
[32m+[m[32m  var old = $.fn.collapse[m
[32m+[m
[32m+[m[32m  $.fn.collapse = function (option) {[m
[32m+[m[32m    return this.each(function () {[m
[32m+[m[32m      var $this = $(this)[m
[32m+[m[32m        , data = $this.data('collapse')[m
[32m+[m[32m        , options = $.extend({}, $.fn.collapse.defaults, $this.data(), typeof option == 'object' && option)[m
[32m+[m[32m      if (!data) $this.data('collapse', (data = new Collapse(this, options)))[m
[32m+[m[32m      if (typeof option == 'string') data[option]()[m
[32m+[m[32m    })[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  $.fn.collapse.defaults = {[m
[32m+[m[32m    toggle: true[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  $.fn.collapse.Constructor = Collapse[m
[32m+[m
[32m+[m
[32m+[m[32m /* COLLAPSE NO CONFLICT[m
[32m+[m[32m  * ==================== */[m
[32m+[m
[32m+[m[32m  $.fn.collapse.noConflict = function () {[m
[32m+[m[32m    $.fn.collapse = old[m
[32m+[m[32m    return this[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m
[32m+[m[32m /* COLLAPSE DATA-API[m
[32m+[m[32m  * ================= */[m
[32m+[m
[32m+[m[32m  $(document).on('click.collapse.data-api', '[data-toggle=collapse]', function (e) {[m
[32m+[m[32m    var $this = $(this), href[m
[32m+[m[32m      , target = $this.attr('data-target')[m
[32m+[m[32m        || e.preventDefault()[m
[32m+[m[32m        || (href = $this.attr('href')) && href.replace(/.*(?=#[^\s]+$)/, '') //strip for ie7[m
[32m+[m[32m      , option = $(target).data('collapse') ? 'toggle' : $this.data()[m
[32m+[m[32m    $this[$(target).hasClass('in') ? 'addClass' : 'removeClass']('collapsed')[m
[32m+[m[32m    $(target).collapse(option)[m
[32m+[m[32m  })[m
[32m+[m
[32m+[m[32m}(window.jQuery);/* ============================================================[m
[32m+[m[32m * bootstrap-dropdown.js v2.3.2[m
[32m+[m[32m * http://getbootstrap.com/2.3.2/javascript.html#dropdowns[m
[32m+[m[32m * ============================================================[m
[32m+[m[32m * Copyright 2013 Twitter, Inc.[m
[32m+[m[32m *[m
[32m+[m[32m * Licensed under the Apache License, Version 2.0 (the "License");[m
[32m+[m[32m * you may not use this file except in compliance with the License.[m
[32m+[m[32m * You may obtain a copy of the License at[m
[32m+[m[32m *[m
[32m+[m[32m * http://www.apache.org/licenses/LICENSE-2.0[m
[32m+[m[32m *[m
[32m+[m[32m * Unless required by applicable law or agreed to in writing, software[m
[32m+[m[32m * distributed under the License is distributed on an "AS IS" BASIS,[m
[32m+[m[32m * WITHOUT WARRANTIES OR CONDITIONS OF ANY KIND, either express or implied.[m
[32m+[m[32m * See the License for the specific language governing permissions and[m
[32m+[m[32m * limitations under the License.[m
[32m+[m[32m * ============================================================ */[m
[32m+[m
[32m+[m
[32m+[m[32m!function ($) {[m
[32m+[m
[32m+[m[32m  "use strict"; // jshint ;_;[m
[32m+[m
[32m+[m
[32m+[m[32m /* DROPDOWN CLASS DEFINITION[m
[32m+[m[32m  * ========================= */[m
[32m+[m
[32m+[m[32m  var toggle = '[data-toggle=dropdown]'[m
[32m+[m[32m    , Dropdown = function (element) {[m
[32m+[m[32m        var $el = $(element).on('click.dropdown.data-api', this.toggle)[m
[32m+[m[32m        $('html').on('click.dropdown.data-api', function () {[m
[32m+[m[32m          $el.parent().removeClass('open')[m
[32m+[m[32m        })[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m  Dropdown.prototype = {[m
[32m+[m
[32m+[m[32m    constructor: Dropdown[m
[32m+[m
[32m+[m[32m  , toggle: function (e) {[m
[32m+[m[32m      var $this = $(this)[m
[32m+[m[32m        , $parent[m
[32m+[m[32m        , isActive[m
[32m+[m
[32m+[m[32m      if ($this.is('.disabled, :disabled')) return[m
[32m+[m
[32m+[m[32m      $parent = getParent($this)[m
[32m+[m
[32m+[m[32m      isActive = $parent.hasClass('open')[m
[32m+[m
[32m+[m[32m      clearMenus()[m
[32m+[m
[32m+[m[32m      if (!isActive) {[m
[32m+[m[32m        if ('ontouchstart' in document.documentElement) {[m
[32m+[m[32m          // if mobile we we use a backdrop because click events don't delegate[m
[32m+[m[32m          $('<div class="dropdown-backdrop"/>').insertBefore($(this)).on('click', clearMenus)[m
[32m+[m[32m        }[m
[32m+[m[32m        $parent.toggleClass('open')[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      $this.focus()[m
[32m+[m
[32m+[m[32m      return false[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , keydown: function (e) {[m
[32m+[m[32m      var $this[m
[32m+[m[32m        , $items[m
[32m+[m[32m        , $active[m
[32m+[m[32m        , $parent[m
[32m+[m[32m        , isActive[m
[32m+[m[32m        , index[m
[32m+[m
[32m+[m[32m      if (!/(38|40|27)/.test(e.keyCode)) return[m
[32m+[m
[32m+[m[32m      $this = $(this)[m
[32m+[m
[32m+[m[32m      e.preventDefault()[m
[32m+[m[32m      e.stopPropagation()[m
[32m+[m
[32m+[m[32m      if ($this.is('.disabled, :disabled')) return[m
[32m+[m
[32m+[m[32m      $parent = getParent($this)[m
[32m+[m
[32m+[m[32m      isActive = $parent.hasClass('open')[m
[32m+[m
[32m+[m[32m      if (!isActive || (isActive && e.keyCode == 27)) {[m
[32m+[m[32m        if (e.which == 27) $parent.find(toggle).focus()[m
[32m+[m[32m        return $this.click()[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      $items = $('[role=menu] li:not(.divider):visible a', $parent)[m
[32m+[m
[32m+[m[32m      if (!$items.length) return[m
[32m+[m
[32m+[m[32m      index = $items.index($items.filter(':focus'))[m
[32m+[m
[32m+[m[32m      if (e.keyCode == 38 && index > 0) index--                                        // up[m
[32m+[m[32m      if (e.keyCode == 40 && index < $items.length - 1) index++                        // down[m
[32m+[m[32m      if (!~index) index = 0[m
[32m+[m
[32m+[m[32m      $items[m
[32m+[m[32m        .eq(index)[m
[32m+[m[32m        .focus()[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function clearMenus() {[m
[32m+[m[32m    $('.dropdown-backdrop').remove()[m
[32m+[m[32m    $(toggle).each(function () {[m
[32m+[m[32m      getParent($(this)).removeClass('open')[m
[32m+[m[32m    })[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function getParent($this) {[m
[32m+[m[32m    var selector = $this.attr('data-target')[m
[32m+[m[32m      , $parent[m
[32m+[m
[32m+[m[32m    if (!selector) {[m
[32m+[m[32m      selector = $this.attr('href')[m
[32m+[m[32m      selector = selector && /#/.test(selector) && selector.replace(/.*(?=#[^\s]*$)/, '') //strip for ie7[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    $parent = selector && $(selector)[m
[32m+[m
[32m+[m[32m    if (!$parent || !$parent.length) $parent = $this.parent()[m
[32m+[m
[32m+[m[32m    return $parent[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m
[32m+[m[32m  /* DROPDOWN PLUGIN DEFINITION[m
[32m+[m[32m   * ========================== */[m
[32m+[m
[32m+[m[32m  var old = $.fn.dropdown[m
[32m+[m
[32m+[m[32m  $.fn.dropdown = function (option) {[m
[32m+[m[32m    return this.each(function () {[m
[32m+[m[32m      var $this = $(this)[m
[32m+[m[32m        , data = $this.data('dropdown')[m
[32m+[m[32m      if (!data) $this.data('dropdown', (data = new Dropdown(this)))[m
[32m+[m[32m      if (typeof option == 'string') data[option].call($this)[m
[32m+[m[32m    })[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  $.fn.dropdown.Constructor = Dropdown[m
[32m+[m
[32m+[m
[32m+[m[32m /* DROPDOWN NO CONFLICT[m
[32m+[m[32m  * ==================== */[m
[32m+[m
[32m+[m[32m  $.fn.dropdown.noConflict = function () {[m
[32m+[m[32m    $.fn.dropdown = old[m
[32m+[m[32m    return this[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m
[32m+[m[32m  /* APPLY TO STANDARD DROPDOWN ELEMENTS[m
[32m+[m[32m   * =================================== */[m
[32m+[m
[32m+[m[32m  $(document)[m
[32m+[m[32m    .on('click.dropdown.data-api', clearMenus)[m
[32m+[m[32m    .on('click.dropdown.data-api', '.dropdown form', function (e) { e.stopPropagation() })[m
[32m+[m[32m    .on('click.dropdown.data-api'  , toggle, Dropdown.prototype.toggle)[m
[32m+[m[32m    .on('keydown.dropdown.data-api', toggle + ', [role=menu]' , Dropdown.prototype.keydown)[m
[32m+[m
[32m+[m[32m}(window.jQuery);[m
[32m+[m[32m/* =========================================================[m
[32m+[m[32m * bootstrap-modal.js v2.3.2[m
[32m+[m[32m * http://getbootstrap.com/2.3.2/javascript.html#modals[m
[32m+[m[32m * =========================================================[m
[32m+[m[32m * Copyright 2013 Twitter, Inc.[m
[32m+[m[32m *[m
[32m+[m[32m * Licensed under the Apache License, Version 2.0 (the "License");[m
[32m+[m[32m * you may not use this file except in compliance with the License.[m
[32m+[m[32m * You may obtain a copy of the License at[m
[32m+[m[32m *[m
[32m+[m[32m * http://www.apache.org/licenses/LICENSE-2.0[m
[32m+[m[32m *[m
[32m+[m[32m * Unless required by applicable law or agreed to in writing, software[m
[32m+[m[32m * distributed under the License is distributed on an "AS IS" BASIS,[m
[32m+[m[32m * WITHOUT WARRANTIES OR CONDITIONS OF ANY KIND, either express or implied.[m
[32m+[m[32m * See the License for the specific language governing permissions and[m
[32m+[m[32m * limitations under the License.[m
[32m+[m[32m * ========================================================= */[m
[32m+[m
[32m+[m
[32m+[m[32m!function ($) {[m
[32m+[m
[32m+[m[32m  "use strict"; // jshint ;_;[m
[32m+[m
[32m+[m
[32m+[m[32m /* MODAL CLASS DEFINITION[m
[32m+[m[32m  * ====================== */[m
[32m+[m
[32m+[m[32m  var Modal = function (element, options) {[m
[32m+[m[32m    this.options = options[m
[32m+[m[32m    this.$element = $(element)[m
[32m+[m[32m      .delegate('[data-dismiss="modal"]', 'click.dismiss.modal', $.proxy(this.hide, this))[m
[32m+[m[32m    this.options.remote && this.$element.find('.modal-body').load(this.options.remote)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  Modal.prototype = {[m
[32m+[m
[32m+[m[32m      constructor: Modal[m
[32m+[m
[32m+[m[32m    , toggle: function () {[m
[32m+[m[32m        return this[!this.isShown ? 'show' : 'hide']()[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m    , show: function () {[m
[32m+[m[32m        var that = this[m
[32m+[m[32m          , e = $.Event('show')[m
[32m+[m
[32m+[m[32m        this.$element.trigger(e)[m
[32m+[m
[32m+[m[32m        if (this.isShown || e.isDefaultPrevented()) return[m
[32m+[m
[32m+[m[32m        this.isShown = true[m
[32m+[m
[32m+[m[32m        this.escape()[m
[32m+[m
[32m+[m[32m        this.backdrop(function () {[m
[32m+[m[32m          var transition = $.support.transition && that.$element.hasClass('fade')[m
[32m+[m
[32m+[m[32m          if (!that.$element.parent().length) {[m
[32m+[m[32m            that.$element.appendTo(document.body) //don't move modals dom position[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          that.$element.show()[m
[32m+[m
[32m+[m[32m          if (transition) {[m
[32m+[m[32m            that.$element[0].offsetWidth // force reflow[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          that.$element[m
[32m+[m[32m            .addClass('in')[m
[32m+[m[32m            .attr('aria-hidden', false)[m
[32m+[m
[32m+[m[32m          that.enforceFocus()[m
[32m+[m
[32m+[m[32m          transition ?[m
[32m+[m[32m            that.$element.one($.support.transition.end, function () { that.$element.focus().trigger('shown') }) :[m
[32m+[m[32m            that.$element.focus().trigger('shown')[m
[32m+[m
[32m+[m[32m        })[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m    , hide: function (e) {[m
[32m+[m[32m        e && e.preventDefault()[m
[32m+[m
[32m+[m[32m        var that = this[m
[32m+[m
[32m+[m[32m        e = $.Event('hide')[m
[32m+[m
[32m+[m[32m        this.$element.trigger(e)[m
[32m+[m
[32m+[m[32m        if (!this.isShown || e.isDefaultPrevented()) return[m
[32m+[m
[32m+[m[32m        this.isShown = false[m
[32m+[m
[32m+[m[32m        this.escape()[m
[32m+[m
[32m+[m[32m        $(document).off('focusin.modal')[m
[32m+[m
[32m+[m[32m        this.$element[m
[32m+[m[32m          .removeClass('in')[m
[32m+[m[32m          .attr('aria-hidden', true)[m
[32m+[m
[32m+[m[32m        $.support.transition && this.$element.hasClass('fade') ?[m
[32m+[m[32m          this.hideWithTransition() :[m
[32m+[m[32m          this.hideModal()[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m    , enforceFocus: function () {[m
[32m+[m[32m        var that = this[m
[32m+[m[32m        $(document).on('focusin.modal', function (e) {[m
[32m+[m[32m          if (that.$element[0] !== e.target && !that.$element.has(e.target).length) {[m
[32m+[m[32m            that.$element.focus()[m
[32m+[m[32m          }[m
[32m+[m[32m        })[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m    , escape: function () {[m
[32m+[m[32m        var that = this[m
[32m+[m[32m        if (this.isShown && this.options.keyboard) {[m
[32m+[m[32m          this.$element.on('keyup.dismiss.modal', function ( e ) {[m
[32m+[m[32m            e.which == 27 && that.hide()[m
[32m+[m[32m          })[m
[32m+[m[32m        } else if (!this.isShown) {[m
[32m+[m[32m          this.$element.off('keyup.dismiss.modal')[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m    , hideWithTransition: function () {[m
[32m+[m[32m        var that = this[m
[32m+[m[32m          , timeout = setTimeout(function () {[m
[32m+[m[32m              that.$element.off($.support.transition.end)[m
[32m+[m[32m              that.hideModal()[m
[32m+[m[32m            }, 500)[m
[32m+[m
[32m+[m[32m        this.$element.one($.support.transition.end, function () {[m
[32m+[m[32m          clearTimeout(timeout)[m
[32m+[m[32m          that.hideModal()[m
[32m+[m[32m        })[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m    , hideModal: function () {[m
[32m+[m[32m        var that = this[m
[32m+[m[32m        this.$element.hide()[m
[32m+[m[32m        this.backdrop(function () {[m
[32m+[m[32m          that.removeBackdrop()[m
[32m+[m[32m          that.$element.trigger('hidden')[m
[32m+[m[32m        })[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m    , removeBackdrop: function () {[m
[32m+[m[32m        this.$backdrop && this.$backdrop.remove()[m
[32m+[m[32m        this.$backdrop = null[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m    , backdrop: function (callback) {[m
[32m+[m[32m        var that = this[m
[32m+[m[32m          , animate = this.$element.hasClass('fade') ? 'fade' : ''[m
[32m+[m
[32m+[m[32m        if (this.isShown && this.options.backdrop) {[m
[32m+[m[32m          var doAnimate = $.support.transition && animate[m
[32m+[m
[32m+[m[32m          this.$backdrop = $('<div class="modal-backdrop ' + animate + '" />')[m
[32m+[m[32m            .appendTo(document.body)[m
[32m+[m
[32m+[m[32m          this.$backdrop.click([m
[32m+[m[32m            this.options.backdrop == 'static' ?[m
[32m+[m[32m              $.proxy(this.$element[0].focus, this.$element[0])[m
[32m+[m[32m            : $.proxy(this.hide, this)[m
[32m+[m[32m          )[m
[32m+[m
[32m+[m[32m          if (doAnimate) this.$backdrop[0].offsetWidth // force reflow[m
[32m+[m
[32m+[m[32m          this.$backdrop.addClass('in')[m
[32m+[m
[32m+[m[32m          if (!callback) return[m
[32m+[m
[32m+[m[32m          doAnimate ?[m
[32m+[m[32m            this.$backdrop.one($.support.transition.end, callback) :[m
[32m+[m[32m            callback()[m
[32m+[m
[32m+[m[32m        } else if (!this.isShown && this.$backdrop) {[m
[32m+[m[32m          this.$backdrop.removeClass('in')[m
[32m+[m
[32m+[m[32m          $.support.transition && this.$element.hasClass('fade')?[m
[32m+[m[32m            this.$backdrop.one($.support.transition.end, callback) :[m
[32m+[m[32m            callback()[m
[32m+[m
[32m+[m[32m        } else if (callback) {[m
[32m+[m[32m          callback()[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m
[32m+[m[32m /* MODAL PLUGIN DEFINITION[m
[32m+[m[32m  * ======================= */[m
[32m+[m
[32m+[m[32m  var old = $.fn.modal[m
[32m+[m
[32m+[m[32m  $.fn.modal = function (option) {[m
[32m+[m[32m    return this.each(function () {[m
[32m+[m[32m      var $this = $(this)[m
[32m+[m[32m        , data = $this.data('modal')[m
[32m+[m[32m        , options = $.extend({}, $.fn.modal.defaults, $this.data(), typeof option == 'object' && option)[m
[32m+[m[32m      if (!data) $this.data('modal', (data = new Modal(this, options)))[m
[32m+[m[32m      if (typeof option == 'string') data[option]()[m
[32m+[m[32m      else if (options.show) data.show()[m
[32m+[m[32m    })[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  $.fn.modal.defaults = {[m
[32m+[m[32m      backdrop: true[m
[32m+[m[32m    , keyboard: true[m
[32m+[m[32m    , show: true[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  $.fn.modal.Constructor = Modal[m
[32m+[m
[32m+[m
[32m+[m[32m /* MODAL NO CONFLICT[m
[32m+[m[32m  * ================= */[m
[32m+[m
[32m+[m[32m  $.fn.modal.noConflict = function () {[m
[32m+[m[32m    $.fn.modal = old[m
[32m+[m[32m    return this[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m
[32m+[m[32m /* MODAL DATA-API[m
[32m+[m[32m  * ============== */[m
[32m+[m
[32m+[m[32m  $(document).on('click.modal.data-api', '[data-toggle="modal"]', function (e) {[m
[32m+[m[32m    var $this = $(this)[m
[32m+[m[32m      , href = $this.attr('href')[m
[32m+[m[32m      , $target = $($this.attr('data-target') || (href && href.replace(/.*(?=#[^\s]+$)/, ''))) //strip for ie7[m
[32m+[m[32m      , option = $target.data('modal') ? 'toggle' : $.extend({ remote:!/#/.test(href) && href }, $target.data(), $this.data())[m
[32m+[m
[32m+[m[32m    e.preventDefault()[m
[32m+[m
[32m+[m[32m    $target[m
[32m+[m[32m      .modal(option)[m
[32m+[m[32m      .one('hide', function () {[m
[32m+[m[32m        $this.focus()[m
[32m+[m[32m      })[m
[32m+[m[32m  })[m
[32m+[m
[32m+[m[32m}(window.jQuery);[m
[32m+[m[32m/* ===========================================================[m
[32m+[m[32m * bootstrap-tooltip.js v2.3.2[m
[32m+[m[32m * http://getbootstrap.com/2.3.2/javascript.html#tooltips[m
[32m+[m[32m * Inspired by the original jQuery.tipsy by Jason Frame[m
[32m+[m[32m * ===========================================================[m
[32m+[m[32m * Copyright 2013 Twitter, Inc.[m
[32m+[m[32m *[m
[32m+[m[32m * Licensed under the Apache License, Version 2.0 (the "License");[m
[32m+[m[32m * you may not use this file except in compliance with the License.[m
[32m+[m[32m * You may obtain a copy of the License at[m
[32m+[m[32m *[m
[32m+[m[32m * http://www.apache.org/licenses/LICENSE-2.0[m
[32m+[m[32m *[m
[32m+[m[32m * Unless required by applicable law or agreed to in writing, software[m
[32m+[m[32m * distributed under the License is distributed on an "AS IS" BASIS,[m
[32m+[m[32m * WITHOUT WARRANTIES OR CONDITIONS OF ANY KIND, either express or implied.[m
[32m+[m[32m * See the License for the specific language governing permissions and[m
[32m+[m[32m * limitations under the License.[m
[32m+[m[32m * ========================================================== */[m
[32m+[m
[32m+[m
[32m+[m[32m!function ($) {[m
[32m+[m
[32m+[m[32m  "use strict"; // jshint ;_;[m
[32m+[m
[32m+[m
[32m+[m[32m /* TOOLTIP PUBLIC CLASS DEFINITION[m
[32m+[m[32m  * =============================== */[m
[32m+[m
[32m+[m[32m  var Tooltip = function (element, options) {[m
[32m+[m[32m    this.init('tooltip', element, options)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  Tooltip.prototype = {[m
[32m+[m
[32m+[m[32m    constructor: Tooltip[m
[32m+[m
[32m+[m[32m  , init: function (type, element, options) {[m
[32m+[m[32m      var eventIn[m
[32m+[m[32m        , eventOut[m
[32m+[m[32m        , triggers[m
[32m+[m[32m        , trigger[m
[32m+[m[32m        , i[m
[32m+[m
[32m+[m[32m      this.type = type[m
[32m+[m[32m      this.$element = $(element)[m
[32m+[m[32m      this.options = this.getOptions(options)[m
[32m+[m[32m      this.enabled = true[m
[32m+[m
[32m+[m[32m      triggers = this.options.trigger.split(' ')[m
[32m+[m
[32m+[m[32m      for (i = triggers.length; i--;) {[m
[32m+[m[32m        trigger = triggers[i][m
[32m+[m[32m        if (trigger == 'click') {[m
[32m+[m[32m          this.$element.on('click.' + this.type, this.options.selector, $.proxy(this.toggle, this))[m
[32m+[m[32m        } else if (trigger != 'manual') {[m
[32m+[m[32m          eventIn = trigger == 'hover' ? 'mouseenter' : 'focus'[m
[32m+[m[32m          eventOut = trigger == 'hover' ? 'mouseleave' : 'blur'[m
[32m+[m[32m          this.$element.on(eventIn + '.' + this.type, this.options.selector, $.proxy(this.enter, this))[m
[32m+[m[32m          this.$element.on(eventOut + '.' + this.type, this.options.selector, $.proxy(this.leave, this))[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      this.options.selector ?[m
[32m+[m[32m        (this._options = $.extend({}, this.options, { trigger: 'manual', selector: '' })) :[m
[32m+[m[32m        this.fixTitle()[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , getOptions: function (options) {[m
[32m+[m[32m      options = $.extend({}, $.fn[this.type].defaults, this.$element.data(), options)[m
[32m+[m
[32m+[m[32m      if (options.delay && typeof options.delay == 'number') {[m
[32m+[m[32m        options.delay = {[m
[32m+[m[32m          show: options.delay[m
[32m+[m[32m        , hide: options.delay[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      return options[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , enter: function (e) {[m
[32m+[m[32m      var defaults = $.fn[this.type].defaults[m
[32m+[m[32m        , options = {}[m
[32m+[m[32m        , self[m
[32m+[m
[32m+[m[32m      this._options && $.each(this._options, function (key, value) {[m
[32m+[m[32m        if (defaults[key] != value) options[key] = value[m
[32m+[m[32m      }, this)[m
[32m+[m
[32m+[m[32m      self = $(e.currentTarget)[this.type](options).data(this.type)[m
[32m+[m
[32m+[m[32m      if (!self.options.delay || !self.options.delay.show) return self.show()[m
[32m+[m
[32m+[m[32m      clearTimeout(this.timeout)[m
[32m+[m[32m      self.hoverState = 'in'[m
[32m+[m[32m      this.timeout = setTimeout(function() {[m
[32m+[m[32m        if (self.hoverState == 'in') self.show()[m
[32m+[m[32m      }, self.options.delay.show)[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , leave: function (e) {[m
[32m+[m[32m      var self = $(e.currentTarget)[this.type](this._options).data(this.type)[m
[32m+[m
[32m+[m[32m      if (this.timeout) clearTimeout(this.timeout)[m
[32m+[m[32m      if (!self.options.delay || !self.options.delay.hide) return self.hide()[m
[32m+[m
[32m+[m[32m      self.hoverState = 'out'[m
[32m+[m[32m      this.timeout = setTimeout(function() {[m
[32m+[m[32m        if (self.hoverState == 'out') self.hide()[m
[32m+[m[32m      }, self.options.delay.hide)[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , show: function () {[m
[32m+[m[32m      var $tip[m
[32m+[m[32m        , pos[m
[32m+[m[32m        , actualWidth[m
[32m+[m[32m        , actualHeight[m
[32m+[m[32m        , placement[m
[32m+[m[32m        , tp[m
[32m+[m[32m        , e = $.Event('show')[m
[32m+[m
[32m+[m[32m      if (this.hasContent() && this.enabled) {[m
[32m+[m[32m        this.$element.trigger(e)[m
[32m+[m[32m        if (e.isDefaultPrevented()) return[m
[32m+[m[32m        $tip = this.tip()[m
[32m+[m[32m        this.setContent()[m
[32m+[m
[32m+[m[32m        if (this.options.animation) {[m
[32m+[m[32m          $tip.addClass('fade')[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        placement = typeof this.options.placement == 'function' ?[m
[32m+[m[32m          this.options.placement.call(this, $tip[0], this.$element[0]) :[m
[32m+[m[32m          this.options.placement[m
[32m+[m
[32m+[m[32m        $tip[m
[32m+[m[32m          .detach()[m
[32m+[m[32m          .css({ top: 0, left: 0, display: 'block' })[m
[32m+[m
[32m+[m[32m        this.options.container ? $tip.appendTo(this.options.container) : $tip.insertAfter(this.$element)[m
[32m+[m
[32m+[m[32m        pos = this.getPosition()[m
[32m+[m
[32m+[m[32m        actualWidth = $tip[0].offsetWidth[m
[32m+[m[32m        actualHeight = $tip[0].offsetHeight[m
[32m+[m
[32m+[m[32m        switch (placement) {[m
[32m+[m[32m          case 'bottom':[m
[32m+[m[32m            tp = {top: pos.top + pos.height, left: pos.left + pos.width / 2 - actualWidth / 2}[m
[32m+[m[32m            break[m
[32m+[m[32m          case 'top':[m
[32m+[m[32m            tp = {top: pos.top - actualHeight, left: pos.left + pos.width / 2 - actualWidth / 2}[m
[32m+[m[32m            break[m
[32m+[m[32m          case 'left':[m
[32m+[m[32m            tp = {top: pos.top + pos.height / 2 - actualHeight / 2, left: pos.left - actualWidth}[m
[32m+[m[32m            break[m
[32m+[m[32m          case 'right':[m
[32m+[m[32m            tp = {top: pos.top + pos.height / 2 - actualHeight / 2, left: pos.left + pos.width}[m
[32m+[m[32m            break[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        this.applyPlacement(tp, placement)[m
[32m+[m[32m        this.$element.trigger('shown')[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , applyPlacement: function(offset, placement){[m
[32m+[m[32m      var $tip = this.tip()[m
[32m+[m[32m        , width = $tip[0].offsetWidth[m
[32m+[m[32m        , height = $tip[0].offsetHeight[m
[32m+[m[32m        , actualWidth[m
[32m+[m[32m        , actualHeight[m
[32m+[m[32m        , delta[m
[32m+[m[32m        , replace[m
[32m+[m
[32m+[m[32m      $tip[m
[32m+[m[32m        .offset(offset)[m
[32m+[m[32m        .addClass(placement)[m
[32m+[m[32m        .addClass('in')[m
[32m+[m
[32m+[m[32m      actualWidth = $tip[0].offsetWidth[m
[32m+[m[32m      actualHeight = $tip[0].offsetHeight[m
[32m+[m
[32m+[m[32m      if (placement == 'top' && actualHeight != height) {[m
[32m+[m[32m        offset.top = offset.top + height - actualHeight[m
[32m+[m[32m        replace = true[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (placement == 'bottom' || placement == 'top') {[m
[32m+[m[32m        delta = 0[m
[32m+[m
[32m+[m[32m        if (offset.left < 0){[m
[32m+[m[32m          delta = offset.left * -2[m
[32m+[m[32m          offset.left = 0[m
[32m+[m[32m          $tip.offset(offset)[m
[32m+[m[32m          actualWidth = $tip[0].offsetWidth[m
[32m+[m[32m          actualHeight = $tip[0].offsetHeight[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        this.replaceArrow(delta - width + actualWidth, actualWidth, 'left')[m
[32m+[m[32m      } else {[m
[32m+[m[32m        this.replaceArrow(actualHeight - height, actualHeight, 'top')[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (replace) $tip.offset(offset)[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , replaceArrow: function(delta, dimension, position){[m
[32m+[m[32m      this[m
[32m+[m[32m        .arrow()[m
[32m+[m[32m        .css(position, delta ? (50 * (1 - delta / dimension) + "%") : '')[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , setContent: function () {[m
[32m+[m[32m      var $tip = this.tip()[m
[32m+[m[32m        , title = this.getTitle()[m
[32m+[m
[32m+[m[32m      $tip.find('.tooltip-inner')[this.options.html ? 'html' : 'text'](title)[m
[32m+[m[32m      $tip.removeClass('fade in top bottom left right')[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , hide: function () {[m
[32m+[m[32m      var that = this[m
[32m+[m[32m        , $tip = this.tip()[m
[32m+[m[32m        , e = $.Event('hide')[m
[32m+[m
[32m+[m[32m      this.$element.trigger(e)[m
[32m+[m[32m      if (e.isDefaultPrevented()) return[m
[32m+[m
[32m+[m[32m      $tip.removeClass('in')[m
[32m+[m
[32m+[m[32m      function removeWithAnimation() {[m
[32m+[m[32m        var timeout = setTimeout(function () {[m
[32m+[m[32m          $tip.off($.support.transition.end).detach()[m
[32m+[m[32m        }, 500)[m
[32m+[m
[32m+[m[32m        $tip.one($.support.transition.end, function () {[m
[32m+[m[32m          clearTimeout(timeout)[m
[32m+[m[32m          $tip.detach()[m
[32m+[m[32m        })[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      $.support.transition && this.$tip.hasClass('fade') ?[m
[32m+[m[32m        removeWithAnimation() :[m
[32m+[m[32m        $tip.detach()[m
[32m+[m
[32m+[m[32m      this.$element.trigger('hidden')[m
[32m+[m
[32m+[m[32m      return this[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , fixTitle: function () {[m
[32m+[m[32m      var $e = this.$element[m
[32m+[m[32m      if ($e.attr('title') || typeof($e.attr('data-original-title')) != 'string') {[m
[32m+[m[32m        $e.attr('data-original-title', $e.attr('title') || '').attr('title', '')[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , hasContent: function () {[m
[32m+[m[32m      return this.getTitle()[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , getPosition: function () {[m
[32m+[m[32m      var el = this.$element[0][m
[32m+[m[32m      return $.extend({}, (typeof el.getBoundingClientRect == 'function') ? el.getBoundingClientRect() : {[m
[32m+[m[32m        width: el.offsetWidth[m
[32m+[m[32m      , height: el.offsetHeight[m
[32m+[m[32m      }, this.$element.offset())[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , getTitle: function () {[m
[32m+[m[32m      var title[m
[32m+[m[32m        , $e = this.$element[m
[32m+[m[32m        , o = this.options[m
[32m+[m
[32m+[m[32m      title = $e.attr('data-original-title')[m
[32m+[m[32m        || (typeof o.title == 'function' ? o.title.call($e[0]) :  o.title)[m
[32m+[m
[32m+[m[32m      return title[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , tip: function () {[m
[32m+[m[32m      return this.$tip = this.$tip || $(this.options.template)[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , arrow: function(){[m
[32m+[m[32m      return this.$arrow = this.$arrow || this.tip().find(".tooltip-arrow")[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , validate: function () {[m
[32m+[m[32m      if (!this.$element[0].parentNode) {[m
[32m+[m[32m        this.hide()[m
[32m+[m[32m        this.$element = null[m
[32m+[m[32m        this.options = null[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , enable: function () {[m
[32m+[m[32m      this.enabled = true[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , disable: function () {[m
[32m+[m[32m      this.enabled = false[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , toggleEnabled: function () {[m
[32m+[m[32m      this.enabled = !this.enabled[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , toggle: function (e) {[m
[32m+[m[32m      var self = e ? $(e.currentTarget)[this.type](this._options).data(this.type) : this[m
[32m+[m[32m      self.tip().hasClass('in') ? self.hide() : self.show()[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , destroy: function () {[m
[32m+[m[32m      this.hide().$element.off('.' + this.type).removeData(this.type)[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m
[32m+[m[32m /* TOOLTIP PLUGIN DEFINITION[m
[32m+[m[32m  * ========================= */[m
[32m+[m
[32m+[m[32m  var old = $.fn.tooltip[m
[32m+[m
[32m+[m[32m  $.fn.tooltip = function ( option ) {[m
[32m+[m[32m    return this.each(function () {[m
[32m+[m[32m      var $this = $(this)[m
[32m+[m[32m        , data = $this.data('tooltip')[m
[32m+[m[32m        , options = typeof option == 'object' && option[m
[32m+[m[32m      if (!data) $this.data('tooltip', (data = new Tooltip(this, options)))[m
[32m+[m[32m      if (typeof option == 'string') data[option]()[m
[32m+[m[32m    })[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  $.fn.tooltip.Constructor = Tooltip[m
[32m+[m
[32m+[m[32m  $.fn.tooltip.defaults = {[m
[32m+[m[32m    animation: true[m
[32m+[m[32m  , placement: 'top'[m
[32m+[m[32m  , selector: false[m
[32m+[m[32m  , template: '<div class="tooltip"><div class="tooltip-arrow"></div><div class="tooltip-inner"></div></div>'[m
[32m+[m[32m  , trigger: 'hover focus'[m
[32m+[m[32m  , title: ''[m
[32m+[m[32m  , delay: 0[m
[32m+[m[32m  , html: false[m
[32m+[m[32m  , container: false[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m
[32m+[m[32m /* TOOLTIP NO CONFLICT[m
[32m+[m[32m  * =================== */[m
[32m+[m
[32m+[m[32m  $.fn.tooltip.noConflict = function () {[m
[32m+[m[32m    $.fn.tooltip = old[m
[32m+[m[32m    return this[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m}(window.jQuery);[m
[32m+[m[32m/* ===========================================================[m
[32m+[m[32m * bootstrap-popover.js v2.3.2[m
[32m+[m[32m * http://getbootstrap.com/2.3.2/javascript.html#popovers[m
[32m+[m[32m * ===========================================================[m
[32m+[m[32m * Copyright 2013 Twitter, Inc.[m
[32m+[m[32m *[m
[32m+[m[32m * Licensed under the Apache License, Version 2.0 (the "License");[m
[32m+[m[32m * you may not use this file except in compliance with the License.[m
[32m+[m[32m * You may obtain a copy of the License at[m
[32m+[m[32m *[m
[32m+[m[32m * http://www.apache.org/licenses/LICENSE-2.0[m
[32m+[m[32m *[m
[32m+[m[32m * Unless required by applicable law or agreed to in writing, software[m
[32m+[m[32m * distributed under the License is distributed on an "AS IS" BASIS,[m
[32m+[m[32m * WITHOUT WARRANTIES OR CONDITIONS OF ANY KIND, either express or implied.[m
[32m+[m[32m * See the License for the specific language governing permissions and[m
[32m+[m[32m * limitations under the License.[m
[32m+[m[32m * =========================================================== */[m
[32m+[m
[32m+[m
[32m+[m[32m!function ($) {[m
[32m+[m
[32m+[m[32m  "use strict"; // jshint ;_;[m
[32m+[m
[32m+[m
[32m+[m[32m /* POPOVER PUBLIC CLASS DEFINITION[m
[32m+[m[32m  * =============================== */[m
[32m+[m
[32m+[m[32m  var Popover = function (element, options) {[m
[32m+[m[32m    this.init('popover', element, options)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m
[32m+[m[32m  /* NOTE: POPOVER EXTENDS BOOTSTRAP-TOOLTIP.js[m
[32m+[m[32m     ========================================== */[m
[32m+[m
[32m+[m[32m  Popover.prototype = $.extend({}, $.fn.tooltip.Constructor.prototype, {[m
[32m+[m
[32m+[m[32m    constructor: Popover[m
[32m+[m
[32m+[m[32m  , setContent: function () {[m
[32m+[m[32m      var $tip = this.tip()[m
[32m+[m[32m        , title = this.getTitle()[m
[32m+[m[32m        , content = this.getContent()[m
[32m+[m
[32m+[m[32m      $tip.find('.popover-title')[this.options.html ? 'html' : 'text'](title)[m
[32m+[m[32m      $tip.find('.popover-content')[this.options.html ? 'html' : 'text'](content)[m
[32m+[m
[32m+[m[32m      $tip.removeClass('fade top bottom left right in')[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , hasContent: function () {[m
[32m+[m[32m      return this.getTitle() || this.getContent()[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , getContent: function () {[m
[32m+[m[32m      var content[m
[32m+[m[32m        , $e = this.$element[m
[32m+[m[32m        , o = this.options[m
[32m+[m
[32m+[m[32m      content = (typeof o.content == 'function' ? o.content.call($e[0]) :  o.content)[m
[32m+[m[32m        || $e.attr('data-content')[m
[32m+[m
[32m+[m[32m      return content[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , tip: function () {[m
[32m+[m[32m      if (!this.$tip) {[m
[32m+[m[32m        this.$tip = $(this.options.template)[m
[32m+[m[32m      }[m
[32m+[m[32m      return this.$tip[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , destroy: function () {[m
[32m+[m[32m      this.hide().$element.off('.' + this.type).removeData(this.type)[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  })[m
[32m+[m
[32m+[m
[32m+[m[32m /* POPOVER PLUGIN DEFINITION[m
[32m+[m[32m  * ======================= */[m
[32m+[m
[32m+[m[32m  var old = $.fn.popover[m
[32m+[m
[32m+[m[32m  $.fn.popover = function (option) {[m
[32m+[m[32m    return this.each(function () {[m
[32m+[m[32m      var $this = $(this)[m
[32m+[m[32m        , data = $this.data('popover')[m
[32m+[m[32m        , options = typeof option == 'object' && option[m
[32m+[m[32m      if (!data) $this.data('popover', (data = new Popover(this, options)))[m
[32m+[m[32m      if (typeof option == 'string') data[option]()[m
[32m+[m[32m    })[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  $.fn.popover.Constructor = Popover[m
[32m+[m
[32m+[m[32m  $.fn.popover.defaults = $.extend({} , $.fn.tooltip.defaults, {[m
[32m+[m[32m    placement: 'right'[m
[32m+[m[32m  , trigger: 'click'[m
[32m+[m[32m  , content: ''[m
[32m+[m[32m  , template: '<div class="popover"><div class="arrow"></div><h3 class="popover-title"></h3><div class="popover-content"></div></div>'[m
[32m+[m[32m  })[m
[32m+[m
[32m+[m
[32m+[m[32m /* POPOVER NO CONFLICT[m
[32m+[m[32m  * =================== */[m
[32m+[m
[32m+[m[32m  $.fn.popover.noConflict = function () {[m
[32m+[m[32m    $.fn.popover = old[m
[32m+[m[32m    return this[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m}(window.jQuery);[m
[32m+[m[32m/* =============================================================[m
[32m+[m[32m * bootstrap-scrollspy.js v2.3.2[m
[32m+[m[32m * http://getbootstrap.com/2.3.2/javascript.html#scrollspy[m
[32m+[m[32m * =============================================================[m
[32m+[m[32m * Copyright 2013 Twitter, Inc.[m
[32m+[m[32m *[m
[32m+[m[32m * Licensed under the Apache License, Version 2.0 (the "License");[m
[32m+[m[32m * you may not use this file except in compliance with the License.[m
[32m+[m[32m * You may obtain a copy of the License at[m
[32m+[m[32m *[m
[32m+[m[32m * http://www.apache.org/licenses/LICENSE-2.0[m
[32m+[m[32m *[m
[32m+[m[32m * Unless required by applicable law or agreed to in writing, software[m
[32m+[m[32m * distributed under the License is distributed on an "AS IS" BASIS,[m
[32m+[m[32m * WITHOUT WARRANTIES OR CONDITIONS OF ANY KIND, either express or implied.[m
[32m+[m[32m * See the License for the specific language governing permissions and[m
[32m+[m[32m * limitations under the License.[m
[32m+[m[32m * ============================================================== */[m
[32m+[m
[32m+[m
[32m+[m[32m!function ($) {[m
[32m+[m
[32m+[m[32m  "use strict"; // jshint ;_;[m
[32m+[m
[32m+[m
[32m+[m[32m /* SCROLLSPY CLASS DEFINITION[m
[32m+[m[32m  * ========================== */[m
[32m+[m
[32m+[m[32m  function ScrollSpy(element, options) {[m
[32m+[m[32m    var process = $.proxy(this.process, this)[m
[32m+[m[32m      , $element = $(element).is('body') ? $(window) : $(element)[m
[32m+[m[32m      , href[m
[32m+[m[32m    this.options = $.extend({}, $.fn.scrollspy.defaults, options)[m
[32m+[m[32m    this.$scrollElement = $element.on('scroll.scroll-spy.data-api', process)[m
[32m+[m[32m    this.selector = (this.options.target[m
[32m+[m[32m      || ((href = $(element).attr('href')) && href.replace(/.*(?=#[^\s]+$)/, '')) //strip for ie7[m
[32m+[m[32m      || '') + ' .nav li > a'[m
[32m+[m[32m    this.$body = $('body')[m
[32m+[m[32m    this.refresh()[m
[32m+[m[32m    this.process()[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  ScrollSpy.prototype = {[m
[32m+[m
[32m+[m[32m      constructor: ScrollSpy[m
[32m+[m
[32m+[m[32m    , refresh: function () {[m
[32m+[m[32m        var self = this[m
[32m+[m[32m          , $targets[m
[32m+[m
[32m+[m[32m        this.offsets = $([])[m
[32m+[m[32m        this.targets = $([])[m
[32m+[m
[32m+[m[32m        $targets = this.$body[m
[32m+[m[32m          .find(this.selector)[m
[32m+[m[32m          .map(function () {[m
[32m+[m[32m            var $el = $(this)[m
[32m+[m[32m              , href = $el.data('target') || $el.attr('href')[m
[32m+[m[32m              , $href = /^#\w/.test(href) && $(href)[m
[32m+[m[32m            return ( $href[m
[32m+[m[32m              && $href.length[m
[32m+[m[32m              && [[ $href.position().top + (!$.isWindow(self.$scrollElement.get(0)) && self.$scrollElement.scrollTop()), href ]] ) || null[m
[32m+[m[32m          })[m
[32m+[m[32m          .sort(function (a, b) { return a[0] - b[0] })[m
[32m+[m[32m          .each(function () {[m
[32m+[m[32m            self.offsets.push(this[0])[m
[32m+[m[32m            self.targets.push(this[1])[m
[32m+[m[32m          })[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m    , process: function () {[m
[32m+[m[32m        var scrollTop = this.$scrollElement.scrollTop() + this.options.offset[m
[32m+[m[32m          , scrollHeight = this.$scrollElement[0].scrollHeight || this.$body[0].scrollHeight[m
[32m+[m[32m          , maxScroll = scrollHeight - this.$scrollElement.height()[m
[32m+[m[32m          , offsets = this.offsets[m
[32m+[m[32m          , targets = this.targets[m
[32m+[m[32m          , activeTarget = this.activeTarget[m
[32m+[m[32m          , i[m
[32m+[m
[32m+[m[32m        if (scrollTop >= maxScroll) {[m
[32m+[m[32m          return activeTarget != (i = targets.last()[0])[m
[32m+[m[32m            && this.activate ( i )[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        for (i = offsets.length; i--;) {[m
[32m+[m[32m          activeTarget != targets[i][m
[32m+[m[32m            && scrollTop >= offsets[i][m
[32m+[m[32m            && (!offsets[i + 1] || scrollTop <= offsets[i + 1])[m
[32m+[m[32m            && this.activate( targets[i] )[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m    , activate: function (target) {[m
[32m+[m[32m        var active[m
[32m+[m[32m          , selector[m
[32m+[m
[32m+[m[32m        this.activeTarget = target[m
[32m+[m
[32m+[m[32m        $(this.selector)[m
[32m+[m[32m          .parent('.active')[m
[32m+[m[32m          .removeClass('active')[m
[32m+[m
[32m+[m[32m        selector = this.selector[m
[32m+[m[32m          + '[data-target="' + target + '"],'[m
[32m+[m[32m          + this.selector + '[href="' + target + '"]'[m
[32m+[m
[32m+[m[32m        active = $(selector)[m
[32m+[m[32m          .parent('li')[m
[32m+[m[32m          .addClass('active')[m
[32m+[m
[32m+[m[32m        if (active.parent('.dropdown-menu').length)  {[m
[32m+[m[32m          active = active.closest('li.dropdown').addClass('active')[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        active.trigger('activate')[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m
[32m+[m[32m /* SCROLLSPY PLUGIN DEFINITION[m
[32m+[m[32m  * =========================== */[m
[32m+[m
[32m+[m[32m  var old = $.fn.scrollspy[m
[32m+[m
[32m+[m[32m  $.fn.scrollspy = function (option) {[m
[32m+[m[32m    return this.each(function () {[m
[32m+[m[32m      var $this = $(this)[m
[32m+[m[32m        , data = $this.data('scrollspy')[m
[32m+[m[32m        , options = typeof option == 'object' && option[m
[32m+[m[32m      if (!data) $this.data('scrollspy', (data = new ScrollSpy(this, options)))[m
[32m+[m[32m      if (typeof option == 'string') data[option]()[m
[32m+[m[32m    })[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  $.fn.scrollspy.Constructor = ScrollSpy[m
[32m+[m
[32m+[m[32m  $.fn.scrollspy.defaults = {[m
[32m+[m[32m    offset: 10[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m
[32m+[m[32m /* SCROLLSPY NO CONFLICT[m
[32m+[m[32m  * ===================== */[m
[32m+[m
[32m+[m[32m  $.fn.scrollspy.noConflict = function () {[m
[32m+[m[32m    $.fn.scrollspy = old[m
[32m+[m[32m    return this[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m
[32m+[m[32m /* SCROLLSPY DATA-API[m
[32m+[m[32m  * ================== */[m
[32m+[m
[32m+[m[32m  $(window).on('load', function () {[m
[32m+[m[32m    $('[data-spy="scroll"]').each(function () {[m
[32m+[m[32m      var $spy = $(this)[m
[32m+[m[32m      $spy.scrollspy($spy.data())[m
[32m+[m[32m    })[m
[32m+[m[32m  })[m
[32m+[m
[32m+[m[32m}(window.jQuery);/* ========================================================[m
[32m+[m[32m * bootstrap-tab.js v2.3.2[m
[32m+[m[32m * http://getbootstrap.com/2.3.2/javascript.html#tabs[m
[32m+[m[32m * ========================================================[m
[32m+[m[32m * Copyright 2013 Twitter, Inc.[m
[32m+[m[32m *[m
[32m+[m[32m * Licensed under the Apache License, Version 2.0 (the "License");[m
[32m+[m[32m * you may not use this file except in compliance with the License.[m
[32m+[m[32m * You may obtain a copy of the License at[m
[32m+[m[32m *[m
[32m+[m[32m * http://www.apache.org/licenses/LICENSE-2.0[m
[32m+[m[32m *[m
[32m+[m[32m * Unless required by applicable law or agreed to in writing, software[m
[32m+[m[32m * distributed under the License is distributed on an "AS IS" BASIS,[m
[32m+[m[32m * WITHOUT WARRANTIES OR CONDITIONS OF ANY KIND, either express or implied.[m
[32m+[m[32m * See the License for the specific language governing permissions and[m
[32m+[m[32m * limitations under the License.[m
[32m+[m[32m * ======================================================== */[m
[32m+[m
[32m+[m
[32m+[m[32m!function ($) {[m
[32m+[m
[32m+[m[32m  "use strict"; // jshint ;_;[m
[32m+[m
[32m+[m
[32m+[m[32m /* TAB CLASS DEFINITION[m
[32m+[m[32m  * ==================== */[m
[32m+[m
[32m+[m[32m  var Tab = function (element) {[m
[32m+[m[32m    this.element = $(element)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  Tab.prototype = {[m
[32m+[m
[32m+[m[32m    constructor: Tab[m
[32m+[m
[32m+[m[32m  , show: function () {[m
[32m+[m[32m      var $this = this.element[m
[32m+[m[32m        , $ul = $this.closest('ul:not(.dropdown-menu)')[m
[32m+[m[32m        , selector = $this.attr('data-target')[m
[32m+[m[32m        , previous[m
[32m+[m[32m        , $target[m
[32m+[m[32m        , e[m
[32m+[m
[32m+[m[32m      if (!selector) {[m
[32m+[m[32m        selector = $this.attr('href')[m
[32m+[m[32m        selector = selector && selector.replace(/.*(?=#[^\s]*$)/, '') //strip for ie7[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if ( $this.parent('li').hasClass('active') ) return[m
[32m+[m
[32m+[m[32m      previous = $ul.find('.active:last a')[0][m
[32m+[m
[32m+[m[32m      e = $.Event('show', {[m
[32m+[m[32m        relatedTarget: previous[m
[32m+[m[32m      })[m
[32m+[m
[32m+[m[32m      $this.trigger(e)[m
[32m+[m
[32m+[m[32m      if (e.isDefaultPrevented()) return[m
[32m+[m
[32m+[m[32m      $target = $(selector)[m
[32m+[m
[32m+[m[32m      this.activate($this.parent('li'), $ul)[m
[32m+[m[32m      this.activate($target, $target.parent(), function () {[m
[32m+[m[32m        $this.trigger({[m
[32m+[m[32m          type: 'shown'[m
[32m+[m[32m        , relatedTarget: previous[m
[32m+[m[32m        })[m
[32m+[m[32m      })[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , activate: function ( element, container, callback) {[m
[32m+[m[32m      var $active = container.find('> .active')[m
[32m+[m[32m        , transition = callback[m
[32m+[m[32m            && $.support.transition[m
[32m+[m[32m            && $active.hasClass('fade')[m
[32m+[m
[32m+[m[32m      function next() {[m
[32m+[m[32m        $active[m
[32m+[m[32m          .removeClass('active')[m
[32m+[m[32m          .find('> .dropdown-menu > .active')[m
[32m+[m[32m          .removeClass('active')[m
[32m+[m
[32m+[m[32m        element.addClass('active')[m
[32m+[m
[32m+[m[32m        if (transition) {[m
[32m+[m[32m          element[0].offsetWidth // reflow for transition[m
[32m+[m[32m          element.addClass('in')[m
[32m+[m[32m        } else {[m
[32m+[m[32m          element.removeClass('fade')[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if ( element.parent('.dropdown-menu') ) {[m
[32m+[m[32m          element.closest('li.dropdown').addClass('active')[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        callback && callback()[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      transition ?[m
[32m+[m[32m        $active.one($.support.transition.end, next) :[m
[32m+[m[32m        next()[m
[32m+[m
[32m+[m[32m      $active.removeClass('in')[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m
[32m+[m[32m /* TAB PLUGIN DEFINITION[m
[32m+[m[32m  * ===================== */[m
[32m+[m
[32m+[m[32m  var old = $.fn.tab[m
[32m+[m
[32m+[m[32m  $.fn.tab = function ( option ) {[m
[32m+[m[32m    return this.each(function () {[m
[32m+[m[32m      var $this = $(this)[m
[32m+[m[32m        , data = $this.data('tab')[m
[32m+[m[32m      if (!data) $this.data('tab', (data = new Tab(this)))[m
[32m+[m[32m      if (typeof option == 'string') data[option]()[m
[32m+[m[32m    })[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  $.fn.tab.Constructor = Tab[m
[32m+[m
[32m+[m
[32m+[m[32m /* TAB NO CONFLICT[m
[32m+[m[32m  * =============== */[m
[32m+[m
[32m+[m[32m  $.fn.tab.noConflict = function () {[m
[32m+[m[32m    $.fn.tab = old[m
[32m+[m[32m    return this[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m
[32m+[m[32m /* TAB DATA-API[m
[32m+[m[32m  * ============ */[m
[32m+[m
[32m+[m[32m  $(document).on('click.tab.data-api', '[data-toggle="tab"], [data-toggle="pill"]', function (e) {[m
[32m+[m[32m    e.preventDefault()[m
[32m+[m[32m    $(this).tab('show')[m
[32m+[m[32m  })[m
[32m+[m
[32m+[m[32m}(window.jQuery);/* =============================================================[m
[32m+[m[32m * bootstrap-typeahead.js v2.3.2[m
[32m+[m[32m * http://getbootstrap.com/2.3.2/javascript.html#typeahead[m
[32m+[m[32m * =============================================================[m
[32m+[m[32m * Copyright 2013 Twitter, Inc.[m
[32m+[m[32m *[m
[32m+[m[32m * Licensed under the Apache License, Version 2.0 (the "License");[m
[32m+[m[32m * you may not use this file except in compliance with the License.[m
[32m+[m[32m * You may obtain a copy of the License at[m
[32m+[m[32m *[m
[32m+[m[32m * http://www.apache.org/licenses/LICENSE-2.0[m
[32m+[m[32m *[m
[32m+[m[32m * Unless required by applicable law or agreed to in writing, software[m
[32m+[m[32m * distributed under the License is distributed on an "AS IS" BASIS,[m
[32m+[m[32m * WITHOUT WARRANTIES OR CONDITIONS OF ANY KIND, either express or implied.[m
[32m+[m[32m * See the License for the specific language governing permissions and[m
[32m+[m[32m * limitations under the License.[m
[32m+[m[32m * ============================================================ */[m
[32m+[m
[32m+[m
[32m+[m[32m!function($){[m
[32m+[m
[32m+[m[32m  "use strict"; // jshint ;_;[m
[32m+[m
[32m+[m
[32m+[m[32m /* TYPEAHEAD PUBLIC CLASS DEFINITION[m
[32m+[m[32m  * ================================= */[m
[32m+[m
[32m+[m[32m  var Typeahead = function (element, options) {[m
[32m+[m[32m    this.$element = $(element)[m
[32m+[m[32m    this.options = $.extend({}, $.fn.typeahead.defaults, options)[m
[32m+[m[32m    this.matcher = this.options.matcher || this.matcher[m
[32m+[m[32m    this.sorter = this.options.sorter || this.sorter[m
[32m+[m[32m    this.highlighter = this.options.highlighter || this.highlighter[m
[32m+[m[32m    this.updater = this.options.updater || this.updater[m
[32m+[m[32m    this.source = this.options.source[m
[32m+[m[32m    this.$menu = $(this.options.menu)[m
[32m+[m[32m    this.shown = false[m
[32m+[m[32m    this.listen()[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  Typeahead.prototype = {[m
[32m+[m
[32m+[m[32m    constructor: Typeahead[m
[32m+[m
[32m+[m[32m  , select: function () {[m
[32m+[m[32m      var val = this.$menu.find('.active').attr('data-value')[m
[32m+[m[32m      this.$element[m
[32m+[m[32m        .val(this.updater(val))[m
[32m+[m[32m        .change()[m
[32m+[m[32m      return this.hide()[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , updater: function (item) {[m
[32m+[m[32m      return item[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , show: function () {[m
[32m+[m[32m      var pos = $.extend({}, this.$element.position(), {[m
[32m+[m[32m        height: this.$element[0].offsetHeight[m
[32m+[m[32m      })[m
[32m+[m
[32m+[m[32m      this.$menu[m
[32m+[m[32m        .insertAfter(this.$element)[m
[32m+[m[32m        .css({[m
[32m+[m[32m          top: pos.top + pos.height[m
[32m+[m[32m        , left: pos.left[m
[32m+[m[32m        })[m
[32m+[m[32m        .show()[m
[32m+[m
[32m+[m[32m      this.shown = true[m
[32m+[m[32m      return this[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , hide: function () {[m
[32m+[m[32m      this.$menu.hide()[m
[32m+[m[32m      this.shown = false[m
[32m+[m[32m      return this[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , lookup: function (event) {[m
[32m+[m[32m      var items[m
[32m+[m
[32m+[m[32m      this.query = this.$element.val()[m
[32m+[m
[32m+[m[32m      if (!this.query || this.query.length < this.options.minLength) {[m
[32m+[m[32m        return this.shown ? this.hide() : this[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      items = $.isFunction(this.source) ? this.source(this.query, $.proxy(this.process, this)) : this.source[m
[32m+[m
[32m+[m[32m      return items ? this.process(items) : this[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , process: function (items) {[m
[32m+[m[32m      var that = this[m
[32m+[m
[32m+[m[32m      items = $.grep(items, function (item) {[m
[32m+[m[32m        return that.matcher(item)[m
[32m+[m[32m      })[m
[32m+[m
[32m+[m[32m      items = this.sorter(items)[m
[32m+[m
[32m+[m[32m      if (!items.length) {[m
[32m+[m[32m        return this.shown ? this.hide() : this[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      return this.render(items.slice(0, this.options.items)).show()[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , matcher: function (item) {[m
[32m+[m[32m      return ~item.toLowerCase().indexOf(this.query.toLowerCase())[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , sorter: function (items) {[m
[32m+[m[32m      var beginswith = [][m
[32m+[m[32m        , caseSensitive = [][m
[32m+[m[32m        , caseInsensitive = [][m
[32m+[m[32m        , item[m
[32m+[m
[32m+[m[32m      while (item = items.shift()) {[m
[32m+[m[32m        if (!item.toLowerCase().indexOf(this.query.toLowerCase())) beginswith.push(item)[m
[32m+[m[32m        else if (~item.indexOf(this.query)) caseSensitive.push(item)[m
[32m+[m[32m        else caseInsensitive.push(item)[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      return beginswith.concat(caseSensitive, caseInsensitive)[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , highlighter: function (item) {[m
[32m+[m[32m      var query = this.query.replace(/[\-\[\]{}()*+?.,\\\^$|#\s]/g, '\\$&')[m
[32m+[m[32m      return item.replace(new RegExp('(' + query + ')', 'ig'), function ($1, match) {[m
[32m+[m[32m        return '<strong>' + match + '</strong>'[m
[32m+[m[32m      })[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , render: function (items) {[m
[32m+[m[32m      var that = this[m
[32m+[m
[32m+[m[32m      items = $(items).map(function (i, item) {[m
[32m+[m[32m        i = $(that.options.item).attr('data-value', item)[m
[32m+[m[32m        i.find('a').html(that.highlighter(item))[m
[32m+[m[32m        return i[0][m
[32m+[m[32m      })[m
[32m+[m
[32m+[m[32m      items.first().addClass('active')[m
[32m+[m[32m      this.$menu.html(items)[m
[32m+[m[32m      return this[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , next: function (event) {[m
[32m+[m[32m      var active = this.$menu.find('.active').removeClass('active')[m
[32m+[m[32m        , next = active.next()[m
[32m+[m
[32m+[m[32m      if (!next.length) {[m
[32m+[m[32m        next = $(this.$menu.find('li')[0])[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      next.addClass('active')[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , prev: function (event) {[m
[32m+[m[32m      var active = this.$menu.find('.active').removeClass('active')[m
[32m+[m[32m        , prev = active.prev()[m
[32m+[m
[32m+[m[32m      if (!prev.length) {[m
[32m+[m[32m        prev = this.$menu.find('li').last()[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      prev.addClass('active')[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , listen: function () {[m
[32m+[m[32m      this.$element[m
[32m+[m[32m        .on('focus',    $.proxy(this.focus, this))[m
[32m+[m[32m        .on('blur',     $.proxy(this.blur, this))[m
[32m+[m[32m        .on('keypress', $.proxy(this.keypress, this))[m
[32m+[m[32m        .on('keyup',    $.proxy(this.keyup, this))[m
[32m+[m
[32m+[m[32m      if (this.eventSupported('keydown')) {[m
[32m+[m[32m        this.$element.on('keydown', $.proxy(this.keydown, this))[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      this.$menu[m
[32m+[m[32m        .on('click', $.proxy(this.click, this))[m
[32m+[m[32m        .on('mouseenter', 'li', $.proxy(this.mouseenter, this))[m
[32m+[m[32m        .on('mouseleave', 'li', $.proxy(this.mouseleave, this))[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , eventSupported: function(eventName) {[m
[32m+[m[32m      var isSupported = eventName in this.$element[m
[32m+[m[32m      if (!isSupported) {[m
[32m+[m[32m        this.$element.setAttribute(eventName, 'return;')[m
[32m+[m[32m        isSupported = typeof this.$element[eventName] === 'function'[m
[32m+[m[32m      }[m
[32m+[m[32m      return isSupported[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , move: function (e) {[m
[32m+[m[32m      if (!this.shown) return[m
[32m+[m
[32m+[m[32m      switch(e.keyCode) {[m
[32m+[m[32m        case 9: // tab[m
[32m+[m[32m        case 13: // enter[m
[32m+[m[32m        case 27: // escape[m
[32m+[m[32m          e.preventDefault()[m
[32m+[m[32m          break[m
[32m+[m
[32m+[m[32m        case 38: // up arrow[m
[32m+[m[32m          e.preventDefault()[m
[32m+[m[32m          this.prev()[m
[32m+[m[32m          break[m
[32m+[m
[32m+[m[32m        case 40: // down arrow[m
[32m+[m[32m          e.preventDefault()[m
[32m+[m[32m          this.next()[m
[32m+[m[32m          break[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      e.stopPropagation()[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , keydown: function (e) {[m
[32m+[m[32m      this.suppressKeyPressRepeat = ~$.inArray(e.keyCode, [40,38,9,13,27])[m
[32m+[m[32m      this.move(e)[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , keypress: function (e) {[m
[32m+[m[32m      if (this.suppressKeyPressRepeat) return[m
[32m+[m[32m      this.move(e)[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , keyup: function (e) {[m
[32m+[m[32m      switch(e.keyCode) {[m
[32m+[m[32m        case 40: // down arrow[m
[32m+[m[32m        case 38: // up arrow[m
[32m+[m[32m        case 16: // shift[m
[32m+[m[32m        case 17: // ctrl[m
[32m+[m[32m        case 18: // alt[m
[32m+[m[32m          break[m
[32m+[m
[32m+[m[32m        case 9: // tab[m
[32m+[m[32m        case 13: // enter[m
[32m+[m[32m          if (!this.shown) return[m
[32m+[m[32m          this.select()[m
[32m+[m[32m          break[m
[32m+[m
[32m+[m[32m        case 27: // escape[m
[32m+[m[32m          if (!this.shown) return[m
[32m+[m[32m          this.hide()[m
[32m+[m[32m          break[m
[32m+[m
[32m+[m[32m        default:[m
[32m+[m[32m          this.lookup()[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      e.stopPropagation()[m
[32m+[m[32m      e.preventDefault()[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  , focus: function (e) {[m
[32m+[m[32m      this.focused = true[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , blur: function (e) {[m
[32m+[m[32m      this.focused = false[m
[32m+[m[32m      if (!this.mousedover && this.shown) this.hide()[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , click: function (e) {[m
[32m+[m[32m      e.stopPropagation()[m
[32m+[m[32m      e.preventDefault()[m
[32m+[m[32m      this.select()[m
[32m+[m[32m      this.$element.focus()[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , mouseenter: function (e) {[m
[32m+[m[32m      this.mousedover = true[m
[32m+[m[32m      this.$menu.find('.active').removeClass('active')[m
[32m+[m[32m      $(e.currentTarget).addClass('active')[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  , mouseleave: function (e) {[m
[32m+[m[32m      this.mousedover = false[m
[32m+[m[32m      if (!this.focused && this.shown) this.hide()[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m
[32m+[m[32m  /* TYPEAHEAD PLUGIN DEFINITION[m
[32m+[m[32m   * =========================== */[m
[32m+[m
[32m+[m[32m  var old = $.fn.typeahead[m
[32m+[m
[32m+[m[32m  $.fn.typeahead = function (option) {[m
[32m+[m[32m    return this.each(function () {[m
[32m+[m[32m      var $this = $(this)[m
[32m+[m[32m        , data = $this.data('typeahead')[m
[32m+[m[32m        , options = typeof option == 'object' && option[m
[32m+[m[32m      if (!data) $this.data('typeahead', (data = new Typeahead(this, options)))[m
[32m+[m[32m      if (typeof option == 'string') data[option]()[m
[32m+[m[32m    })[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  $.fn.typeahead.defaults = {[m
[32m+[m[32m    source: [][m
[32m+[m[32m  , items: 8[m
[32m+[m[32m  , menu: '<ul class="typeahead dropdown-menu"></ul>'[m
[32m+[m[32m  , item: '<li><a href="#"></a></li>'[m
[32m+[m[32m  , minLength: 1[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  $.fn.typeahead.Constructor = Typeahead[m
[32m+[m
[32m+[m
[32m+[m[32m /* TYPEAHEAD NO CONFLICT[m
[32m+[m[32m  * =================== */[m
[32m+[m
[32m+[m[32m  $.fn.typeahead.noConflict = function () {[m
[32m+[m[32m    $.fn.typeahead = old[m
[32m+[m[32m    return this[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m
[32m+[m[32m /* TYPEAHEAD DATA-API[m
[32m+[m[32m  * ================== */[m
[32m+[m
[32m+[m[32m  $(document).on('focus.typeahead.data-api', '[data-provide="typeahead"]', function (e) {[m
[32m+[m[32m    var $this = $(this)[m
[32m+[m[32m    if ($this.data('typeahead')) return[m
[32m+[m[32m    $this.typeahead($this.data())[m
[32m+[m[32m  })[m
[32m+[m
[32m+[m[32m}(window.jQuery);[m
[32m+[m[32m/* ==========================================================[m
[32m+[m[32m * bootstrap-affix.js v2.3.2[m
[32m+[m[32m * http://getbootstrap.com/2.3.2/javascript.html#affix[m
[32m+[m[32m * ==========================================================[m
[32m+[m[32m * Copyright 2013 Twitter, Inc.[m
[32m+[m[32m *[m
[32m+[m[32m * Licensed under the Apache License, Version 2.0 (the "License");[m
[32m+[m[32m * you may not use this file except in compliance with the License.[m
[32m+[m[32m * You may obtain a copy of the License at[m
[32m+[m[32m *[m
[32m+[m[32m * http://www.apache.org/licenses/LICENSE-2.0[m
[32m+[m[32m *[m
[32m+[m[32m * Unless required by applicable law or agreed to in writing, software[m
[32m+[m[32m * distributed under the License is distributed on an "AS IS" BASIS,[m
[32m+[m[32m * WITHOUT WARRANTIES OR CONDITIONS OF ANY KIND, either express or implied.[m
[32m+[m[32m * See the License for the specific language governing permissions and[m
[32m+[m[32m * limitations under the License.[m
[32m+[m[32m * ========================================================== */[m
[32m+[m
[32m+[m
[32m+[m[32m!function ($) {[m
[32m+[m
[32m+[m[32m  "use strict"; // jshint ;_;[m
[32m+[m
[32m+[m
[32m+[m[32m /* AFFIX CLASS DEFINITION[m
[32m+[m[32m  * ====================== */[m
[32m+[m
[32m+[m[32m  var Affix = function (element, options) {[m
[32m+[m[32m    this.options = $.extend({}, $.fn.affix.defaults, options)[m
[32m+[m[32m    this.$window = $(window)[m
[32m+[m[32m      .on('scroll.affix.data-api', $.proxy(this.checkPosition, this))[m
[32m+[m[32m      .on('click.affix.data-api',  $.proxy(function () { setTimeout($.proxy(this.checkPosition, this), 1) }, this))[m
[32m+[m[32m    this.$element = $(element)[m
[32m+[m[32m    this.checkPosition()[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  Affix.prototype.checkPosition = function () {[m
[32m+[m[32m    if (!this.$element.is(':visible')) return[m
[32m+[m
[32m+[m[32m    var scrollHeight = $(document).height()[m
[32m+[m[32m      , scrollTop = this.$window.scrollTop()[m
[32m+[m[32m      , position = this.$element.offset()[m
[32m+[m[32m      , offset = this.options.offset[m
[32m+[m[32m      , offsetBottom = offset.bottom[m
[32m+[m[32m      , offsetTop = offset.top[m
[32m+[m[32m      , reset = 'affix affix-top affix-bottom'[m
[32m+[m[32m      , affix[m
[32m+[m
[32m+[m[32m    if (typeof offset != 'object') offsetBottom = offsetTop = offset[m
[32m+[m[32m    if (typeof offsetTop == 'function') offsetTop = offset.top()[m
[32m+[m[32m    if (typeof offsetBottom == 'function') offsetBottom = offset.bottom()[m
[32m+[m
[32m+[m[32m    affix = this.unpin != null && (scrollTop + this.unpin <= position.top) ?[m
[32m+[m[32m      false    : offsetBottom != null && (position.top + this.$element.height() >= scrollHeight - offsetBottom) ?[m
[32m+[m[32m      'bottom' : offsetTop != null && scrollTop <= offsetTop ?[m
[32m+[m[32m      'top'    : false[m
[32m+[m
[32m+[m[32m    if (this.affixed === affix) return[m
[32m+[m
[32m+[m[32m    this.affixed = affix[m
[32m+[m[32m    this.unpin = affix == 'bottom' ? position.top - scrollTop : null[m
[32m+[m
[32m+[m[32m    this.$element.removeClass(reset).addClass('affix' + (affix ? '-' + affix : ''))[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m
[32m+[m[32m /* AFFIX PLUGIN DEFINITION[m
[32m+[m[32m  * ======================= */[m
[32m+[m
[32m+[m[32m  var old = $.fn.affix[m
[32m+[m
[32m+[m[32m  $.fn.affix = function (option) {[m
[32m+[m[32m    return this.each(function () {[m
[32m+[m[32m      var $this = $(this)[m
[32m+[m[32m        , data = $this.data('affix')[m
[32m+[m[32m        , options = typeof option == 'object' && option[m
[32m+[m[32m      if (!data) $this.data('affix', (data = new Affix(this, options)))[m
[32m+[m[32m      if (typeof option == 'string') data[option]()[m
[32m+[m[32m    })[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  $.fn.affix.Constructor = Affix[m
[32m+[m
[32m+[m[32m  $.fn.affix.defaults = {[m
[32m+[m[32m    offset: 0[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m
[32m+[m[32m /* AFFIX NO CONFLICT[m
[32m+[m[32m  * ================= */[m
[32m+[m
[32m+[m[32m  $.fn.affix.noConflict = function () {[m
[32m+[m[32m    $.fn.affix = old[m
[32m+[m[32m    return this[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m
[32m+[m[32m /* AFFIX DATA-API[m
[32m+[m[32m  * ============== */[m
[32m+[m
[32m+[m[32m  $(window).on('load', function () {[m
[32m+[m[32m    $('[data-spy="affix"]').each(function () {[m
[32m+[m[32m      var $spy = $(this)[m
[32m+[m[32m        , data = $spy.data()[m
[32m+[m
[32m+[m[32m      data.offset = data.offset || {}[m
[32m+[m
[32m+[m[32m      data.offsetBottom && (data.offset.bottom = data.offsetBottom)[m
[32m+[m[32m      data.offsetTop && (data.offset.top = data.offsetTop)[m
[32m+[m
[32m+[m[32m      $spy.affix(data)[m
[32m+[m[32m    })[m
[32m+[m[32m  })[m
[32m+[m
[32m+[m
[32m+[m[32m}(window.jQuery);[m
\ No newline at end of file[m
