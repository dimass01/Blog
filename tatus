[32m+[m[32m</div>[m
[32m+[m
[32m+[m
[32m+[m[32m{# On charge la bibliothÃ¨que jQuery. Ici, je la prends depuis le site jquery.com,[m
[32m+[m[32m   mais si vous l'avez en local, changez simplement l'adresse. #}[m
[32m+[m[32m<script src="{{ asset('js/jquery-1.11.0.js') }}"></script>[m
[32m+[m[32m{# Voici le script en question : #}[m
[32m+[m[32m<script type="text/javascript">[m
[32m+[m[32m$(document).ready(function() {[m
[32m+[m[32m  // On rÃ©cupÃ¨re la balise <div> en question qui contient l'attribut Â« data-prototype Â» qui nous intÃ©resse.[m
[32m+[m[32m  var $container = $('div#sdz_blogbundle_articletype_categories');[m
[32m+[m[32m  // On ajoute un lien pour ajouter une nouvelle catÃ©gorie[m
[32m+[m[32m  var $lienAjout = $('<a href="#" id="ajout_categorie" class="btn">Ajouter une catÃ©gorie</a>');[m
[32m+[m[32m  $container.append($lienAjout);[m
[32m+[m[32m  // On ajoute un nouveau champ Ã  chaque clic sur le lien d'ajout.[m
[32m+[m[32m  $lienAjout.click(function(e) {[m
[32m+[m[32m    ajouterCategorie($container);[m
[32m+[m[32m    e.preventDefault(); // Ã©vite qu'un # apparaisse dans l'URL[m
[32m+[m[32m    return false;[m
[32m+[m[32m  });[m
[32m+[m[32m  // On dÃ©finit un compteur unique pour nommer les champs qu'on va ajouter dynamiquement[m
[32m+[m[32m  var index = $container.find(':input').length;[m
[32m+[m[32m  // On ajoute un premier champ directement s'il n'en existe pas dÃ©jÃ  un (cas d'un nouvel article par exemple).[m
[32m+[m[32m  if (index == 0) {[m
[32m+[m[32m    ajouterCategorie($container);[m
[32m+[m[32m  } else {[m
[32m+[m[32m    // Pour chaque catÃ©gorie dÃ©jÃ  existante, on ajoute un lien de suppression[m
[32m+[m[32m    $container.children('div').each(function() {[m
[32m+[m[32m      ajouterLienSuppression($(this));[m
[32m+[m[32m    });[m
[32m+[m[32m  }[m
[32m+[m[32m  // La fonction qui ajoute un formulaire Categorie[m
[32m+[m[32m  function ajouterCategorie($container) {[m
[32m+[m[32m    // Dans le contenu de l'attribut Â« data-prototype Â», on remplace :[m
[32m+[m[32m    // - le texte "__name__label__" qu'il contient par le label du champ[m
[32m+[m[32m    // - le texte "__name__" qu'il contient par le numÃ©ro du champ[m
[32m+[m[32m    var $prototype = $($container.attr('data-prototype').replace(/__name__label__/g, 'CatÃ©gorie nÂ°' + (index+1))[m
[32m+[m[32m                                                        .replace(/__name__/g, index));[m
[32m+[m[32m    // On ajoute au prototype un lien pour pouvoir supprimer la catÃ©gorie[m
[32m+[m[32m    ajouterLienSuppression($prototype);[m
[32m+[m[32m    // On ajoute le prototype modifiÃ© Ã  la fin de la balise <div>[m
[32m+[m[32m    $container.append($prototype);[m
[32m+[m[32m    // Enfin, on incrÃ©mente le compteur pour que le prochain ajout se fasse avec un autre numÃ©ro[m
[32m+[m[32m    index++;[m
[32m+[m[32m  }[m
[32m+[m[32m  // La fonction qui ajoute un lien de suppression d'une catÃ©gorie[m
[32m+[m[32m  function ajouterLienSuppression($prototype) {[m
[32m+[m[32m    // CrÃ©ation du lien[m
[32m+[m[32m    $lienSuppression = $('<a href="#" class="btn btn-danger">Supprimer</a>');[m
[32m+[m[32m    // Ajout du lien[m
[32m+[m[32m    $prototype.append($lienSuppression);[m
[32m+[m[32m    // Ajout du listener sur le clic du lien[m
[32m+[m[32m    $lienSuppression.click(function(e) {[m
[32m+[m[32m      $prototype.remove();[m
[32m+[m[32m      e.preventDefault(); // Ã©vite qu'un # apparaisse dans l'URL[m
[32m+[m[32m      return false;[m
[32m+[m[32m    });[m
[32m+[m[32m  }[m
[32m+[m[32m});[m
[32m+[m[32m</script>[m
