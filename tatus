[1mdiff --git a/app/SymfonyRequirements.php b/app/SymfonyRequirements.php[m
[1mnew file mode 100644[m
[1mindex 0000000..fb70f80[m
[1m--- /dev/null[m
[1m+++ b/app/SymfonyRequirements.php[m
[36m@@ -0,0 +1,687 @@[m
[32m+[m[32m<?php[m
[32m+[m
[32m+[m[32m/*[m
[32m+[m[32m * This file is part of the Symfony package.[m
[32m+[m[32m *[m
[32m+[m[32m * (c) Fabien Potencier <fabien@symfony.com>[m
[32m+[m[32m *[m
[32m+[m[32m * For the full copyright and license information, please view the LICENSE[m
[32m+[m[32m * file that was distributed with this source code.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m/*[m
[32m+[m[32m * Users of PHP 5.2 should be able to run the requirements checks.[m
[32m+[m[32m * This is why the file and all classes must be compatible with PHP 5.2+[m
[32m+[m[32m * (e.g. not using namespaces and closures).[m
[32m+[m[32m *[m
[32m+[m[32m * ************** CAUTION **************[m
[32m+[m[32m *[m
[32m+[m[32m * DO NOT EDIT THIS FILE as it will be overridden by Composer as part of[m
[32m+[m[32m * the installation/update process. The original file resides in the[m
[32m+[m[32m * SensioDistributionBundle.[m
[32m+[m[32m *[m
[32m+[m[32m * ************** CAUTION **************[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Represents a single PHP requirement, e.g. an installed extension.[m
[32m+[m[32m * It can be a mandatory requirement or an optional recommendation.[m
[32m+[m[32m * There is a special subclass, named PhpIniRequirement, to check a php.ini configuration.[m
[32m+[m[32m *[m
[32m+[m[32m * @author Tobias Schultze <http://tobion.de>[m
[32m+[m[32m */[m
[32m+[m[32mclass Requirement[m
[32m+[m[32m{[m
[32m+[m[32m    private $fulfilled;[m
[32m+[m[32m    private $testMessage;[m
[32m+[m[32m    private $helpText;[m
[32m+[m[32m    private $helpHtml;[m
[32m+[m[32m    private $optional;[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Constructor that initializes the requirement.[m
[32m+[m[32m     *[m
[32m+[m[32m     * @param Boolean     $fulfilled   Whether the requirement is fulfilled[m
[32m+[m[32m     * @param string      $testMessage The message for testing the requirement[m
[32m+[m[32m     * @param string      $helpHtml    The help text formatted in HTML for resolving the problem[m
[32m+[m[32m     * @param string|null $helpText    The help text (when null, it will be inferred from $helpHtml, i.e. stripped from HTML tags)[m
[32m+[m[32m     * @param Boolean     $optional    Whether this is only an optional recommendation not a mandatory requirement[m
[32m+[m[32m     */[m
[32m+[m[32m    public function __construct($fulfilled, $testMessage, $helpHtml, $helpText = null, $optional = false)[m
[32m+[m[32m    {[m
[32m+[m[32m        $this->fulfilled = (Boolean) $fulfilled;[m
[32m+[m[32m        $this->testMessage = (string) $testMessage;[m
[32m+[m[32m        $this->helpHtml = (string) $helpHtml;[m
[32m+[m[32m        $this->helpText = null === $helpText ? strip_tags($this->helpHtml) : (string) $helpText;[m
[32m+[m[32m        $this->optional = (Boolean) $optional;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Returns whether the requirement is fulfilled.[m
[32m+[m[32m     *[m
[32m+[m[32m     * @return Boolean true if fulfilled, otherwise false[m
[32m+[m[32m     */[m
[32m+[m[32m    public function isFulfilled()[m
[32m+[m[32m    {[m
[32m+[m[32m        return $this->fulfilled;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Returns the message for testing the requirement.[m
[32m+[m[32m     *[m
[32m+[m[32m     * @return string The test message[m
[32m+[m[32m     */[m
[32m+[m[32m    public function getTestMessage()[m
[32m+[m[32m    {[m
[32m+[m[32m        return $this->testMessage;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Returns the help text for resolving the problem[m
[32m+[m[32m     *[m
[32m+[m[32m     * @return string The help text[m
[32m+[m[32m     */[m
[32m+[m[32m    public function getHelpText()[m
[32m+[m[32m    {[m
[32m+[m[32m        return $this->helpText;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Returns the help text formatted in HTML.[m
[32m+[m[32m     *[m
[32m+[m[32m     * @return string The HTML help[m
[32m+[m[32m     */[m
[32m+[m[32m    public function getHelpHtml()[m
[32m+[m[32m    {[m
[32m+[m[32m        return $this->helpHtml;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Returns whether this is only an optional recommendation and not a mandatory requirement.[m
[32m+[m[32m     *[m
[32m+[m[32m     * @return Boolean true if optional, false if mandatory[m
[32m+[m[32m     */[m
[32m+[m[32m    public function isOptional()[m
[32m+[m[32m    {[m
[32m+[m[32m        return $this->optional;[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Represents a PHP requirement in form of a php.ini configuration.[m
[32m+[m[32m *[m
[32m+[m[32m * @author Tobias Schultze <http://tobion.de>[m
[32m+[m[32m */[m
[32m+[m[32mclass PhpIniRequirement extends Requirement[m
[32m+[m[32m{[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Constructor that initializes the requirement.[m
[32m+[m[32m     *[m
[32m+[m[32m     * @param string           $cfgName    The configuration name used for ini_get()[m
[32m+[m[32m     * @param Boolean|callback $evaluation Either a Boolean indicating whether the configuration should evaluate to true or false,[m
[32m+[m[32m                                                    or a callback function receiving the configuration value as parameter to determine the fulfillment of the requirement[m
[32m+[m[32m     * @param Boolean $approveCfgAbsence If true the Requirement will be fulfilled even if the configuration option does not exist, i.e. ini_get() returns false.[m
[32m+[m[32m                                                    This is helpful for abandoned configs in later PHP versions or configs of an optional extension, like Suhosin.[m
[32m+[m[32m                                                    Example: You require a config to be true but PHP later removes this config and defaults it to true internally.[m
[32m+[m[32m     * @param string|null $testMessage The message for testing the requirement (when null and $evaluation is a Boolean a default message is derived)[m
[32m+[m[32m     * @param string|null $helpHtml    The help text formatted in HTML for resolving the problem (when null and $evaluation is a Boolean a default help is derived)[m
[32m+[m[32m     * @param string|null $helpText    The help text (when null, it will be inferred from $helpHtml, i.e. stripped from HTML tags)[m
[32m+[m[32m     * @param Boolean     $optional    Whether this is only an optional recommendation not a mandatory requirement[m
[32m+[m[32m     */[m
[32m+[m[32m    public function __construct($cfgName, $evaluation, $approveCfgAbsence = false, $testMessage = null, $helpHtml = null, $helpText = null, $optional = false)[m
[32m+[m[32m    {[m
[32m+[m[32m        $cfgValue = ini_get($cfgName);[m
[32m+[m
[32m+[m[32m        if (is_callable($evaluation)) {[m
[32m+[m[32m            if (null === $testMessage || null === $helpHtml) {[m
[32m+[m[32m                throw new InvalidArgumentException('You must provide the parameters testMessage and helpHtml for a callback evaluation.');[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            $fulfilled = call_user_func($evaluation, $cfgValue);[m
[32m+[m[32m        } else {[m
[32m+[m[32m            if (null === $testMessage) {[m
[32m+[m[32m                $testMessage = sprintf('%s %s be %s in php.ini',[m
[32m+[m[32m                    $cfgName,[m
[32m+[m[32m                    $optional ? 'should' : 'must',[m
[32m+[m[32m                    $evaluation ? 'enabled' : 'disabled'[m
[32m+[m[32m                );[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            if (null === $helpHtml) {[m
[32m+[m[32m                $helpHtml = sprintf('Set <strong>%s</strong> to <strong>%s</strong> in php.ini<a href="#phpini">*</a>.',[m
[32m+[m[32m                    $cfgName,[m
[32m+[m[32m                    $evaluation ? 'on' : 'off'[m
[32m+[m[32m                );[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            $fulfilled = $evaluation == $cfgValue;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        parent::__construct($fulfilled || ($approveCfgAbsence && false === $cfgValue), $testMessage, $helpHtml, $helpText, $optional);[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * A RequirementCollection represents a set of Requirement instances.[m
[32m+[m[32m *[m
[32m+[m[32m * @author Tobias Schultze <http://tobion.de>[m
[32m+[m[32m */[m
[32m+[m[32mclass RequirementCollection implements IteratorAggregate[m
[32m+[m[32m{[m
[32m+[m[32m    private $requirements = array();[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Gets the current RequirementCollection as an Iterator.[m
[32m+[m[32m     *[m
[32m+[m[32m     * @return Traversable A Traversable interface[m
[32m+[m[32m     */[m
[32m+[m[32m    public function getIterator()[m
[32m+[m[32m    {[m
[32m+[m[32m        return new ArrayIterator($this->requirements);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Adds a Requirement.[m
[32m+[m[32m     *[m
[32m+[m[32m     * @param Requirement $requirement A Requirement instance[m
[32m+[m[32m     */[m
[32m+[m[32m    public function add(Requirement $requirement)[m
[32m+[m[32m    {[m
[32m+[m[32m        $this->requirements[] = $requirement;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Adds a mandatory requirement.[m
[32m+[m[32m     *[m
[32m+[m[32m     * @param Boolean     $fulfilled   Whether the requirement is fulfilled[m
[32m+[m[32m     * @param string      $testMessage The message for testing the requirement[m
[32m+[m[32m     * @param string      $helpHtml    The help text formatted in HTML for resolving the problem[m
[32m+[m[32m     * @param string|null $helpText    The help text (when null, it will be inferred from $helpHtml, i.e. stripped from HTML tags)[m
[32m+[m[32m     */[m
[32m+[m[32m    public function addRequirement($fulfilled, $testMessage, $helpHtml, $helpText = null)[m
[32m+[m[32m    {[m
[32m+[m[32m        $this->add(new Requirement($fulfilled, $testMessage, $helpHtml, $helpText, false));[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Adds an optional recommendation.[m
[32m+[m[32m     *[m
[32m+[m[32m     * @param Boolean     $fulfilled   Whether the recommendation is fulfilled[m
[32m+[m[32m     * @param string      $testMessage The message for testing the recommendation[m
[32m+[m[32m     * @param string      $helpHtml    The help text formatted in HTML for resolving the problem[m
[32m+[m[32m     * @param string|null $helpText    The help text (when null, it will be inferred from $helpHtml, i.e. stripped from HTML tags)[m
[32m+[m[32m     */[m
[32m+[m[32m    public function addRecommendation($fulfilled, $testMessage, $helpHtml, $helpText = null)[m
[32m+[m[32m    {[m
[32m+[m[32m        $this->add(new Requirement($fulfilled, $testMessage, $helpHtml, $helpText, true));[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Adds a mandatory requirement in form of a php.ini configuration.[m
[32m+[m[32m     *[m
[32m+[m[32m     * @param string           $cfgName    The configuration name used for ini_get()[m
[32m+[m[32m     * @param Boolean|callback $evaluation Either a Boolean indicating whether the configuration should evaluate to true or false,[m
[32m+[m[32m                                                    or a callback function receiving the configuration value as parameter to determine the fulfillment of the requirement[m
[32m+[m[32m     * @param Boolean $approveCfgAbsence If true the Requirement will be fulfilled even if the configuration option does not exist, i.e. ini_get() returns false.[m
[32m+[m[32m                                                    This is helpful for abandoned configs in later PHP versions or configs of an optional extension, like Suhosin.[m
[32m+[m[32m                                                    Example: You require a config to be true but PHP later removes this config and defaults it to true internally.[m
[32m+[m[32m     * @param string      $testMessage The message for testing the requirement (when null and $evaluation is a Boolean a default message is derived)[m
[32m+[m[32m     * @param string      $helpHtml    The help text formatted in HTML for resolving the problem (when null and $evaluation is a Boolean a default help is derived)[m
[32m+[m[32m     * @param string|null $helpText    The help text (when null, it will be inferred from $helpHtml, i.e. stripped from HTML tags)[m
[32m+[m[32m     */[m
[32m+[m[32m    public function addPhpIniRequirement($cfgName, $evaluation, $approveCfgAbsence = false, $testMessage = null, $helpHtml = null, $helpText = null)[m
[32m+[m[32m    {[m
[32m+[m[32m        $this->add(new PhpIniRequirement($cfgName, $evaluation, $approveCfgAbsence, $testMessage, $helpHtml, $helpText, false));[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Adds an optional recommendation in form of a php.ini configuration.[m
[32m+[m[32m     *[m
[32m+[m[32m     * @param string           $cfgName    The configuration name used for ini_get()[m
[32m+[m[32m     * @param Boolean|callback $evaluation Either a Boolean indicating whether the configuration should evaluate to true or false,[m
[32m+[m[32m                                                    or a callback function receiving the configuration value as parameter to determine the fulfillment of the requirement[m
[32m+[m[32m     * @param Boolean $approveCfgAbsence If true the Requirement will be fulfilled even if the configuration option does not exist, i.e. ini_get() returns false.[m
[32m+[m[32m                                                    This is helpful for abandoned configs in later PHP versions or configs of an optional extension, like Suhosin.[m
[32m+[m[32m                                                    Example: You require a config to be true but PHP later removes this config and defaults it to true internally.[m
[32m+[m[32m     * @param string      $testMessage The message for testing the requirement (when null and $evaluation is a Boolean a default message is derived)[m
[32m+[m[32m     * @param string      $helpHtml    The help text formatted in HTML for resolving the problem (when null and $evaluation is a Boolean a default help is derived)[m
[32m+[m[32m     * @param string|null $helpText    The help text (when null, it will be inferred from $helpHtml, i.e. stripped from HTML tags)[m
[32m+[m[32m     */[m
[32m+[m[32m    public function addPhpIniRecommendation($cfgName, $evaluation, $approveCfgAbsence = false, $testMessage = null, $helpHtml = null, $helpText = null)[m
[32m+[m[32m    {[m
[32m+[m[32m        $this->add(new PhpIniRequirement($cfgName, $evaluation, $approveCfgAbsence, $testMessage, $helpHtml, $helpText, true));[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Adds a requirement collection to the current set of requirements.[m
[32m+[m[32m     *[m
[32m+[m[32m     * @param RequirementCollection $collection A RequirementCollection instance[m
[32m+[m[32m     */[m
[32m+[m[32m    public function addCollection(RequirementCollection $collection)[m
[32m+[m[32m    {[m
[32m+[m[32m        $this->requirements = array_merge($this->requirements, $collection->all());[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Returns both requirements and recommendations.[m
[32m+[m[32m     *[m
[32m+[m[32m     * @return array Array of Requirement instances[m
[32m+[m[32m     */[m
[32m+[m[32m    public function all()[m
[32m+[m[32m    {[m
[32m+[m[32m        return $this->requirements;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Returns all mandatory requirements.[m
[32m+[m[32m     *[m
[32m+[m[32m     * @return array Array of Requirement instances[m
[32m+[m[32m     */[m
[32m+[m[32m    public function getRequirements()[m
[32m+[m[32m    {[m
[32m+[m[32m        $array = array();[m
[32m+[m[32m        foreach ($this->requirements as $req) {[m
[32m+[m[32m            if (!$req->isOptional()) {[m
[32m+[m[32m                $array[] = $req;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return $array;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Returns the mandatory requirements that were not met.[m
[32m+[m[32m     *[m
[32m+[m[32m     * @return array Array of Requirement instances[m
[32m+[m[32m     */[m
[32m+[m[32m    public function getFailedRequirements()[m
[32m+[m[32m    {[m
[32m+[m[32m        $array = array();[m
[32m+[m[32m        foreach ($this->requirements as $req) {[m
[32m+[m[32m            if (!$req->isFulfilled() && !$req->isOptional()) {[m
[32m+[m[32m                $array[] = $req;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return $array;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Returns all optional recommendations.[m
[32m+[m[32m     *[m
[32m+[m[32m     * @return array Array of Requirement instances[m
[32m+[m[32m     */[m
[32m+[m[32m    public function getRecommendations()[m
[32m+[m[32m    {[m
[32m+[m[32m        $array = array();[m
[32m+[m[32m        foreach ($this->requirements as $req) {[m
[32m+[m[32m            if ($req->isOptional()) {[m
[32m+[m[32m                $array[] = $req;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return $array;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Returns the recommendations that were not met.[m
[32m+[m[32m     *[m
[32m+[m[32m     * @return array Array of Requirement instances[m
[32m+[m[32m     */[m
[32m+[m[32m    public function getFailedRecommendations()[m
[32m+[m[32m    {[m
[32m+[m[32m        $array = array();[m
[32m+[m[32m        foreach ($this->requirements as $req) {[m
[32m+[m[32m            if (!$req->isFulfilled() && $req->isOptional()) {[m
[32m+[m[32m                $array[] = $req;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return $array;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Returns whether a php.ini configuration is not correct.[m
[32m+[m[32m     *[m
[32m+[m[32m     * @return Boolean php.ini configuration problem?[m
[32m+[m[32m     */[m
[32m+[m[32m    public function hasPhpIniConfigIssue()[m
[32m+[m[32m    {[m
[32m+[m[32m        foreach ($this->requirements as $req) {[m
[32m+[m[32m            if (!$req->isFulfilled() && $req instanceof PhpIniRequirement) {[m
[32m+[m[32m                return true;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return false;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Returns the PHP configuration file (php.ini) path.[m
[32m+[m[32m     *[m
[32m+[m[32m     * @return string|false php.ini file path[m
[32m+[m[32m     */[m
[32m+[m[32m    public function getPhpIniConfigPath()[m
[32m+[m[32m    {[m
[32m+[m[32m        return get_cfg_var('cfg_file_path');[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * This class specifies all requirements and optional recommendations that[m
[32m+[m[32m * are necessary to run the Symfony Standard Edition.[m
[32m+[m[32m *[m
[32m+[m[32m * @author Tobias Schultze <http://tobion.de>[m
[32m+[m[32m * @author Fabien Potencier <fabien@symfony.com>[m
[32m+[m[32m */[m
[32m+[m[32mclass SymfonyRequirements extends RequirementCollection[m
[32m+[m[32m{[m
[32m+[m[32m    const REQUIRED_PHP_VERSION = '5.3.3';[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Constructor that initializes the requirements.[m
[32m+[m[32m     */[m
[32m+[m[32m    public function __construct()[m
[32m+[m[32m    {[m
[32m+[m[32m        /* mandatory requirements follow */[m
[32m+[m
[32m+[m[32m        $installedPhpVersion = phpversion();[m
[32m+[m
[32m+[m[32m        $this->addRequirement([m
[32m+[m[32m            version_compare($installedPhpVersion, self::REQUIRED_PHP_VERSION, '>='),[m
[32m+[m[32m            sprintf('PHP version must be at least %s (%s installed)', self::REQUIRED_PHP_VERSION, $installedPhpVersion),[m
[32m+[m[32m            sprintf('You are running PHP version "<strong>%s</strong>", but Symfony needs at least PHP "<strong>%s</strong>" to run.[m
[32m+[m[32m                Before using Symfony, upgrade your PHP installation, preferably to the latest version.',[m
[32m+[m[32m                $installedPhpVersion, self::REQUIRED_PHP_VERSION),[m
[32m+[m[32m            sprintf('Install PHP %s or newer (installed version is %s)', self::REQUIRED_PHP_VERSION, $installedPhpVersion)[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        $this->addRequirement([m
[32m+[m[32m            version_compare($installedPhpVersion, '5.3.16', '!='),[m
[32m+[m[32m            'PHP version must not be 5.3.16 as Symfony won\'t work properly with it',[m
[32m+[m[32m            'Install PHP 5.3.17 or newer (or downgrade to an earlier PHP version)'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        $this->addRequirement([m
[32m+[m[32m            is_dir(__DIR__.'/../vendor/composer'),[m
[32m+[m[32m            'Vendor libraries must be installed',[m
[32m+[m[32m            'Vendor libraries are missing. Install composer following instructions from <a href="http://getcomposer.org/">http://getcomposer.org/</a>. ' .[m
[32m+[m[32m                'Then run "<strong>php composer.phar install</strong>" to install them.'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        $baseDir = basename(__DIR__);[m
[32m+[m
[32m+[m[32m        $this->addRequirement([m
[32m+[m[32m            is_writable(__DIR__.'/cache'),[m
[32m+[m[32m            "$baseDir/cache/ directory must be writable",[m
[32m+[m[32m            "Change the permissions of the \"<strong>$baseDir/cache/</strong>\" directory so that the web server can write into it."[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        $this->addRequirement([m
[32m+[m[32m            is_writable(__DIR__.'/logs'),[m
[32m+[m[32m            "$baseDir/logs/ directory must be writable",[m
[32m+[m[32m            "Change the permissions of the \"<strong>$baseDir/logs/</strong>\" directory so that the web server can write into it."[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        $this->addPhpIniRequirement([m
[32m+[m[32m            'date.timezone', true, false,[m
[32m+[m[32m            'date.timezone setting must be set',[m
[32m+[m[32m            'Set the "<strong>date.timezone</strong>" setting in php.ini<a href="#phpini">*</a> (like Europe/Paris).'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        if (version_compare($installedPhpVersion, self::REQUIRED_PHP_VERSION, '>=')) {[m
[32m+[m[32m            $timezones = array();[m
[32m+[m[32m            foreach (DateTimeZone::listAbbreviations() as $abbreviations) {[m
[32m+[m[32m                foreach ($abbreviations as $abbreviation) {[m
[32m+[m[32m                    $timezones[$abbreviation['timezone_id']] = true;[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            $this->addRequirement([m
[32m+[m[32m                isset($timezones[date_default_timezone_get()]),[m
[32m+[m[32m                sprintf('Configured default timezone "%s" must be supported by your installation of PHP', date_default_timezone_get()),[m
[32m+[m[32m                'Your default timezone is not supported by PHP. Check for typos in your <strong>php.ini</strong> file and have a look at the list of deprecated timezones at <a href="http://php.net/manual/en/timezones.others.php">http://php.net/manual/en/timezones.others.php</a>.'[m
[32m+[m[32m            );[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        $this->addRequirement([m
[32m+[m[32m            function_exists('json_encode'),[m
[32m+[m[32m            'json_encode() must be available',[m
[32m+[m[32m            'Install and enable the <strong>JSON</strong> extension.'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        $this->addRequirement([m
[32m+[m[32m            function_exists('session_start'),[m
[32m+[m[32m            'session_start() must be available',[m
[32m+[m[32m            'Install and enable the <strong>session</strong> extension.'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        $this->addRequirement([m
[32m+[m[32m            function_exists('ctype_alpha'),[m
[32m+[m[32m            'ctype_alpha() must be available',[m
[32m+[m[32m            'Install and enable the <strong>ctype</strong> extension.'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        $this->addRequirement([m
[32m+[m[32m            function_exists('token_get_all'),[m
[32m+[m[32m            'token_get_all() must be available',[m
[32m+[m[32m            'Install and enable the <strong>Tokenizer</strong> extension.'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        $this->addRequirement([m
[32m+[m[32m            function_exists('simplexml_import_dom'),[m
[32m+[m[32m            'simplexml_import_dom() must be available',[m
[32m+[m[32m            'Install and enable the <strong>SimpleXML</strong> extension.'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        if (function_exists('apc_store') && ini_get('apc.enabled')) {[m
[32m+[m[32m            if (version_compare($installedPhpVersion, '5.4.0', '>=')) {[m
[32m+[m[32m                $this->addRequirement([m
[32m+[m[32m                    version_compare(phpversion('apc'), '3.1.13', '>='),[m
[32m+[m[32m                    'APC version must be at least 3.1.13 when using PHP 5.4',[m
[32m+[m[32m                    'Upgrade your <strong>APC</strong> extension (3.1.13+).'[m
[32m+[m[32m                );[m
[32m+[m[32m            } else {[m
[32m+[m[32m                $this->addRequirement([m
[32m+[m[32m                    version_compare(phpversion('apc'), '3.0.17', '>='),[m
[32m+[m[32m                    'APC version must be at least 3.0.17',[m
[32m+[m[32m                    'Upgrade your <strong>APC</strong> extension (3.0.17+).'[m
[32m+[m[32m                );[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        $this->addPhpIniRequirement('detect_unicode', false);[m
[32m+[m
[32m+[m[32m        if (extension_loaded('suhosin')) {[m
[32m+[m[32m            $this->addPhpIniRequirement([m
[32m+[m[32m                'suhosin.executor.include.whitelist',[m
[32m+[m[32m                create_function('$cfgValue', 'return false !== stripos($cfgValue, "phar");'),[m
[32m+[m[32m                false,[m
[32m+[m[32m                'suhosin.executor.include.whitelist must be configured correctly in php.ini',[m
[32m+[m[32m                'Add "<strong>phar</strong>" to <strong>suhosin.executor.include.whitelist</strong> in php.ini<a href="#phpini">*</a>.'[m
[32m+[m[32m            );[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (extension_loaded('xdebug')) {[m
[32m+[m[32m            $this->addPhpIniRequirement([m
[32m+[m[32m                'xdebug.show_exception_trace', false, true[m
[32m+[m[32m            );[m
[32m+[m
[32m+[m[32m            $this->addPhpIniRequirement([m
[32m+[m[32m                'xdebug.scream', false, true[m
[32m+[m[32m            );[m
[32m+[m
[32m+[m[32m            $this->addPhpIniRecommendation([m
[32m+[m[32m                'xdebug.max_nesting_level',[m
[32m+[m[32m                create_function('$cfgValue', 'return $cfgValue > 100;'),[m
[32m+[m[32m                true,[m
[32m+[m[32m                'xdebug.max_nesting_level should be above 100 in php.ini',[m
[32m+[m[32m                'Set "<strong>xdebug.max_nesting_level</strong>" to e.g. "<strong>250</strong>" in php.ini<a href="#phpini">*</a> to stop Xdebug\'s infinite recursion protection erroneously throwing a fatal error in your project.'[m
[32m+[m[32m            );[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        $pcreVersion = defined('PCRE_VERSION') ? (float) PCRE_VERSION : null;[m
[32m+[m
[32m+[m[32m        $this->addRequirement([m
[32m+[m[32m            null !== $pcreVersion,[m
[32m+[m[32m            'PCRE extension must be available',[m
[32m+[m[32m            'Install the <strong>PCRE</strong> extension (version 8.0+).'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        /* optional recommendations follow */[m
[32m+[m
[32m+[m[32m        $this->addRecommendation([m
[32m+[m[32m            file_get_contents(__FILE__) === file_get_contents(__DIR__.'/../vendor/sensio/distribution-bundle/Sensio/Bundle/DistributionBundle/Resources/skeleton/app/SymfonyRequirements.php'),[m
[32m+[m[32m            'Requirements file should be up-to-date',[m
[32m+[m[32m            'Your requirements file is outdated. Run composer install and re-check your configuration.'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        $this->addRecommendation([m
[32m+[m[32m            version_compare($installedPhpVersion, '5.3.4', '>='),[m
[32m+[m[32m            'You should use at least PHP 5.3.4 due to PHP bug #52083 in earlier versions',[m
[32m+[m[32m            'Your project might malfunction randomly due to PHP bug #52083 ("Notice: Trying to get property of non-object"). Install PHP 5.3.4 or newer.'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        $this->addRecommendation([m
[32m+[m[32m            version_compare($installedPhpVersion, '5.3.8', '>='),[m
[32m+[m[32m            'When using annotations you should have at least PHP 5.3.8 due to PHP bug #55156',[m
[32m+[m[32m            'Install PHP 5.3.8 or newer if your project uses annotations.'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        $this->addRecommendation([m
[32m+[m[32m            version_compare($installedPhpVersion, '5.4.0', '!='),[m
[32m+[m[32m            'You should not use PHP 5.4.0 due to the PHP bug #61453',[m
[32m+[m[32m            'Your project might not work properly due to the PHP bug #61453 ("Cannot dump definitions which have method calls"). Install PHP 5.4.1 or newer.'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        $this->addRecommendation([m
[32m+[m[32m            version_compare($installedPhpVersion, '5.4.11', '>='),[m
[32m+[m[32m            'When using the logout handler from the Symfony Security Component, you should have at least PHP 5.4.11 due to PHP bug #63379 (as a workaround, you can also set invalidate_session to false in the security logout handler configuration)',[m
[32m+[m[32m            'Install PHP 5.4.11 or newer if your project uses the logout handler from the Symfony Security Component.'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        $this->addRecommendation([m
[32m+[m[32m            (version_compare($installedPhpVersion, '5.3.18', '>=') && version_compare($installedPhpVersion, '5.4.0', '<'))[m
[32m+[m[32m            ||[m
[32m+[m[32m            version_compare($installedPhpVersion, '5.4.8', '>='),[m
[32m+[m[32m            'You should use PHP 5.3.18+ or PHP 5.4.8+ to always get nice error messages for fatal errors in the development environment due to PHP bug #61767/#60909',[m
[32m+[m[32m            'Install PHP 5.3.18+ or PHP 5.4.8+ if you want nice error messages for all fatal errors in the development environment.'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        if (null !== $pcreVersion) {[m
[32m+[m[32m            $this->addRecommendation([m
[32m+[m[32m                $pcreVersion >= 8.0,[m
[32m+[m[32m                sprintf('PCRE extension should be at least version 8.0 (%s installed)', $pcreVersion),[m
[32m+[m[32m                '<strong>PCRE 8.0+</strong> is preconfigured in PHP since 5.3.2 but you are using an outdated version of it. Symfony probably works anyway but it is recommended to upgrade your PCRE extension.'[m
[32m+[m[32m            );[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        $this->addRecommendation([m
[32m+[m[32m            class_exists('DomDocument'),[m
[32m+[m[32m            'PHP-XML module should be installed',[m
[32m+[m[32m            'Install and enable the <strong>PHP-XML</strong> module.'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        $this->addRecommendation([m
[32m+[m[32m            function_exists('mb_strlen'),[m
[32m+[m[32m            'mb_strlen() should be available',[m
[32m+[m[32m            'Install and enable the <strong>mbstring</strong> extension.'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        $this->addRecommendation([m
[32m+[m[32m            function_exists('iconv'),[m
[32m+[m[32m            'iconv() should be available',[m
[32m+[m[32m            'Install and enable the <strong>iconv</strong> extension.'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        $this->addRecommendation([m
[32m+[m[32m            function_exists('utf8_decode'),[m
[32m+[m[32m            'utf8_decode() should be available',[m
[32m+[m[32m            'Install and enable the <strong>XML</strong> extension.'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        if (!defined('PHP_WINDOWS_VERSION_BUILD')) {[m
[32m+[m[32m            $this->addRecommendation([m
[32m+[m[32m                function_exists('posix_isatty'),[m
[32m+[m[32m                'posix_isatty() should be available',[m
[32m+[m[32m                'Install and enable the <strong>php_posix</strong> extension (used to colorize the CLI output).'[m
[32m+[m[32m            );[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        $this->addRecommendation([m
[32m+[m[32m            class_exists('Locale'),[m
[32m+[m[32m            'intl extension should be available',[m
[32m+[m[32m            'Install and enable the <strong>intl</strong> extension (used for validators).'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        if (class_exists('Collator')) {[m
[32m+[m[32m            $this->addRecommendation([m
[32m+[m[32m                null !== new Collator('fr_FR'),[m
[32m+[m[32m                'intl extension should be correctly configured',[m
[32m+[m[32m                'The intl extension does not behave properly. This problem is typical on PHP 5.3.X x64 WIN builds.'[m
[32m+[m[32m            );[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (class_exists('Locale')) {[m
[32m+[m[32m            if (defined('INTL_ICU_VERSION')) {[m
[32m+[m[32m                $version = INTL_ICU_VERSION;[m
[32m+[m[32m            } else {[m
[32m+[m[32m                $reflector = new ReflectionExtension('intl');[m
[32m+[m
[32m+[m[32m                ob_start();[m
[32m+[m[32m                $reflector->info();[m
[32m+[m[32m                $output = strip_tags(ob_get_clean());[m
[32m+[m
[32m+[m[32m                preg_match('/^ICU version +(?:=> )?(.*)$/m', $output, $matches);[m
[32m+[m[32m                $version = $matches[1];[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            $this->addRecommendation([m
[32m+[m[32m                version_compare($version, '4.0', '>='),[m
[32m+[m[32m                'intl ICU version should be at least 4+',[m
[32m+[m[32m                'Upgrade your <strong>intl</strong> extension with a newer ICU version (4+).'[m
[32m+[m[32m            );[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        $accelerator =[m
[32m+[m[32m            (extension_loaded('eaccelerator') && ini_get('eaccelerator.enable'))[m
[32m+[m[32m            ||[m
[32m+[m[32m            (extension_loaded('apc') && ini_get('apc.enabled'))[m
[32m+[m[32m            ||[m
[32m+[m[32m            (extension_loaded('Zend OPcache') && ini_get('opcache.enable'))[m
[32m+[m[32m            ||[m
[32m+[m[32m            (extension_loaded('xcache') && ini_get('xcache.cacher'))[m
[32m+[m[32m            ||[m
[32m+[m[32m            (extension_loaded('wincache') && ini_get('wincache.ocenabled'))[m
[32m+[m[32m        ;[m
[32m+[m
[32m+[m[32m        $this->addRecommendation([m
[32m+[m[32m            $accelerator,[m
[32m+[m[32m            'a PHP accelerator should be installed',[m
[32m+[m[32m            'Install and enable a <strong>PHP accelerator</strong> like APC (highly recommended).'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        $this->addPhpIniRecommendation('short_open_tag', false);[m
[32m+[m
[32m+[m[32m        $this->addPhpIniRecommendation('magic_quotes_gpc', false, true);[m
[32m+[m
[32m+[m[32m        $this->addPhpIniRecommendation('register_globals', false, true);[m
[32m+[m
[32m+[m[32m        $this->addPhpIniRecommendation('session.auto_start', false);[m
[32m+[m
[32m+[m[32m        $this->addRecommendation([m
[32m+[m[32m            class_exists('PDO'),[m
[32m+[m[32m            'PDO should be installed',[m
[32m+[m[32m            'Install <strong>PDO</strong> (mandatory for Doctrine).'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        if (class_exists('PDO')) {[m
[32m+[m[32m            $drivers = PDO::getAvailableDrivers();[m
[32m+[m[32m            $this->addRecommendation([m
[32m+[m[32m                count($drivers),[m
[32m+[m[32m                sprintf('PDO should have some drivers installed (currently available: %s)', count($drivers) ? implode(', ', $drivers) : 'none'),[m
[32m+[m[32m                'Install <strong>PDO drivers</strong> (mandatory for Doctrine).'[m
[32m+[m[32m            );[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
