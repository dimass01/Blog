[32m+[m[32m//Request::enableHttpMethodParameterOverride();[m
[32m+[m[32m$request = Request::createFromGlobals();[m
[32m+[m[32m$response = $kernel->handle($request);[m
[32m+[m[32m$response->send();[m
[32m+[m[32m$kernel->terminate($request, $response);[m
