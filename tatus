[32m+[m
[32m+[m[32mnamespace Sdz\Bundle\BlogBundle\Entity;[m
[32m+[m
[32m+[m[32muse Doctrine\ORM\Mapping as ORM;[m
[32m+[m[32muse Gedmo\Mapping\Annotation as Gedmo;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Article[m
[32m+[m[32m *[m
[32m+[m[32m * @ORM\Table(name="sdz_article")[m
[32m+[m[32m * @ORM\Entity(repositoryClass="Sdz\Bundle\BlogBundle\Entity\ArticleRepository")[m
[32m+[m[32m * @ORM\HasLifecycleCallbacks()[m
[32m+[m[32m */[m
[32m+[m[32mclass Article[m
[32m+[m[32m{[m
[32m+[m[32m    /**[m
[32m+[m[32m     * @var integer[m
[32m+[m[32m     *[m
[32m+[m[32m     * @ORM\Column(name="id", type="integer")[m
[32m+[m[32m     * @ORM\Id[m
[32m+[m[32m     * @ORM\GeneratedValue(strategy="AUTO")[m
[32m+[m[32m     */[m
[32m+[m[32m    private $id;[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * @var \DateTime[m
[32m+[m[32m     *[m
[32m+[m[32m     * @ORM\Column(name="date", type="datetime")[m
[32m+[m[32m     */[m
[32m+[m[32m    private $date;[m
[32m+[m
[32m+[m[32m    /**[m
