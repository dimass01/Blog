[1mdiff --git a/web/.htaccess b/web/.htaccess[m
[1mnew file mode 100644[m
[1mindex 0000000..0c5f37d[m
[1m--- /dev/null[m
[1m+++ b/web/.htaccess[m
[36m@@ -0,0 +1,52 @@[m
[32m+[m[32m# Use the front controller as index file. It serves as a fallback solution when[m
[32m+[m[32m# every other rewrite/redirect fails (e.g. in an aliased environment without[m
[32m+[m[32m# mod_rewrite). Additionally, this reduces the matching process for the[m
[32m+[m[32m# start page (path "/") because otherwise Apache will apply the rewriting rules[m
[32m+[m[32m# to each configured DirectoryIndex file (e.g. index.php, index.html, index.pl).[m
[32m+[m[32mDirectoryIndex app.php[m
[32m+[m
[32m+[m[32m<IfModule mod_rewrite.c>[m
[32m+[m[32m    RewriteEngine On[m
[32m+[m
[32m+[m[32m    # Determine the RewriteBase automatically and set it as environment variable.[m
[32m+[m[32m    # If you are using Apache aliases to do mass virtual hosting or installed the[m
[32m+[m[32m    # project in a subdirectory, the base path will be prepended to allow proper[m
[32m+[m[32m    # resolution of the app.php file and to redirect to the correct URI. It will[m
[32m+[m[32m    # work in environments without path prefix as well, providing a safe, one-size[m
[32m+[m[32m    # fits all solution. But as you do not need it in this case, you can comment[m
[32m+[m[32m    # the following 2 lines to eliminate the overhead.[m
[32m+[m[32m    RewriteCond %{REQUEST_URI}::$1 ^(/.+)/(.*)::\2$[m
[32m+[m[32m    RewriteRule ^(.*) - [E=BASE:%1][m
[32m+[m
[32m+[m[32m    # Redirect to URI without front controller to prevent duplicate content[m
[32m+[m[32m    # (with and without `/app.php`). Only do this redirect on the initial[m
[32m+[m[32m    # rewrite by Apache and not on subsequent cycles. Otherwise we would get an[m
[32m+[m[32m    # endless redirect loop (request -> rewrite to front controller ->[m
[32m+[m[32m    # redirect -> request -> ...).[m
[32m+[m[32m    # So in case you get a "too many redirects" error or you always get redirected[m
[32m+[m[32m    # to the start page because your Apache does not expose the REDIRECT_STATUS[m
[32m+[m[32m    # environment variable, you have 2 choices:[m
[32m+[m[32m    # - disable this feature by commenting the following 2 lines or[m
[32m+[m[32m    # - use Apache >= 2.3.9 and replace all L flags by END flags and remove the[m
[32m+[m[32m    #   following RewriteCond (best solution)[m
[32m+[m[32m    RewriteCond %{ENV:REDIRECT_STATUS} ^$[m
[32m+[m[32m    RewriteRule ^app\.php(/(.*)|$) %{ENV:BASE}/$2 [R=301,L][m
[32m+[m
[32m+[m[32m    # If the requested filename exists, simply serve it.[m
[32m+[m[32m    # We only want to let Apache serve files and not directories.[m
[32m+[m[32m    RewriteCond %{REQUEST_FILENAME} -f[m
[32m+[m[32m    RewriteRule .? - [L][m
[32m+[m
[32m+[m[32m    # Rewrite all other queries to the front controller.[m
[32m+[m[32m    RewriteRule .? %{ENV:BASE}/app.php [L][m
[32m+[m[32m</IfModule>[m
[32m+[m
[32m+[m[32m<IfModule !mod_rewrite.c>[m
[32m+[m[32m    <IfModule mod_alias.c>[m
[32m+[m[32m        # When mod_rewrite is not available, we instruct a temporary redirect of[m
[32m+[m[32m        # the start page to the front controller explicitly so that the website[m
[32m+[m[32m        # and the generated links can still be used.[m
[32m+[m[32m        RedirectMatch 302 ^/$ /app.php/[m
[32m+[m[32m        # RedirectTemp cannot be used instead[m
[32m+[m[32m    </IfModule>[m
[32m+[m[32m</IfModule>[m
[1mdiff --git a/web/app.php b/web/app.php[m
[1mnew file mode 100644[m
[1mindex 0000000..4f591c1[m
[1m--- /dev/null[m
[1m+++ b/web/app.php[m
[36m@@ -0,0 +1,29 @@[m
