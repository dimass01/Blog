[32m+[m[32m    public function getSelectedCategories(array $selectedCategories)[m
[32m+[m[32m     {[m
[32m+[m[32m       $qb = $this->createQueryBuilder('c');[m
[32m+[m[32m       // On fait une jointure avec l'entitÃ© Categorie, avec pour alias Â« c Â»[m
[32m+[m[32m       $qb->where($qb->expr()->in('c.nom', $selectedCategories)); // Puis on filtre sur le nom des catÃ©gories Ã  l'aide d'un IN[m
[32m+[m[32m       // Enfin, on retourne le rÃ©sultat[m
[32m+[m[32m       return $qb;[m
[32m+[m[32m     }[m
[32m+[m[41m [m
[32m+[m[32m}[m
