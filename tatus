[1mdiff --git a/app/check.php b/app/check.php[m
[1mnew file mode 100644[m
[1mindex 0000000..91b826b[m
[1m--- /dev/null[m
[1m+++ b/app/check.php[m
[36m@@ -0,0 +1,62 @@[m
[32m+[m[32m<?php[m
[32m+[m
[32m+[m[32mrequire_once dirname(__FILE__).'/SymfonyRequirements.php';[m
[32m+[m
[32m+[m[32m$symfonyRequirements = new SymfonyRequirements();[m
[32m+[m
[32m+[m[32m$iniPath = $symfonyRequirements->getPhpIniConfigPath();[m
[32m+[m
[32m+[m[32mecho "********************************\n";[m
[32m+[m[32mecho "*                              *\n";[m
[32m+[m[32mecho "*  Symfony requirements check  *\n";[m
[32m+[m[32mecho "*                              *\n";[m
[32m+[m[32mecho "********************************\n\n";[m
[32m+[m
[32m+[m[32mecho $iniPath ? sprintf("* Configuration file used by PHP: %s\n\n", $iniPath) : "* WARNING: No configuration file (php.ini) used by PHP!\n\n";[m
[32m+[m
[32m+[m[32mecho "** ATTENTION **\n";[m
[32m+[m[32mecho "*  The PHP CLI can use a different php.ini file\n";[m
[32m+[m[32mecho "*  than the one used with your web server.\n";[m
[32m+[m[32mif ('\\' == DIRECTORY_SEPARATOR) {[m
[32m+[m[32m    echo "*  (especially on the Windows platform)\n";[m
[32m+[m[32m}[m
[32m+[m[32mecho "*  To be on the safe side, please also launch the requirements check\n";[m
[32m+[m[32mecho "*  from your web server using the web/config.php script.\n";[m
[32m+[m
[32m+[m[32mecho_title('Mandatory requirements');[m
[32m+[m
[32m+[m[32m$checkPassed = true;[m
[32m+[m[32mforeach ($symfonyRequirements->getRequirements() as $req) {[m
[32m+[m[32m    /** @var $req Requirement */[m
[32m+[m[32m    echo_requirement($req);[m
[32m+[m[32m    if (!$req->isFulfilled()) {[m
[32m+[m[32m        $checkPassed = false;[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mecho_title('Optional recommendations');[m
[32m+[m
[32m+[m[32mforeach ($symfonyRequirements->getRecommendations() as $req) {[m
[32m+[m[32m    echo_requirement($req);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mexit($checkPassed ? 0 : 1);[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Prints a Requirement instance[m
[32m+[m[32m */[m
[32m+[m[32mfunction echo_requirement(Requirement $requirement)[m
[32m+[m[32m{[m
[32m+[m[32m    $result = $requirement->isFulfilled() ? 'OK' : ($requirement->isOptional() ? 'WARNING' : 'ERROR');[m
[32m+[m[32m    echo ' ' . str_pad($result, 9);[m
[32m+[m[32m    echo $requirement->getTestMessage() . "\n";[m
[32m+[m
[32m+[m[32m    if (!$requirement->isFulfilled()) {[m
[32m+[m[32m        echo sprintf("          %s\n\n", $requirement->getHelpText());[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction echo_title($title)[m
[32m+[m[32m{[m
[32m+[m[32m    echo "\n** $title **\n\n";[m
[32m+[m[32m}[m
