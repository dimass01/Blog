[1m+++ b/src/Acme/DemoBundle/Command/HelloWorldCommand.php[m
[36m@@ -0,0 +1,48 @@[m
[32m+[m[32m<?php[m
[32m+[m
[32m+[m[32mnamespace Acme\DemoBundle\Command;[m
[32m+[m
[32m+[m[32muse Symfony\Component\Console\Command\Command;[m
[32m+[m[32muse Symfony\Component\Console\Input\InputArgument;[m
[32m+[m[32muse Symfony\Component\Console\Input\InputInterface;[m
[32m+[m[32muse Symfony\Component\Console\Output\OutputInterface;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Hello World command for demo purposes.[m
[32m+[m[32m *[m
[32m+[m[32m * You could also extend from Symfony\Bundle\FrameworkBundle\Command\ContainerAwareCommand[m
[32m+[m[32m * to get access to the container via $this->getContainer().[m
[32m+[m[32m *[m
[32m+[m[32m * @author Tobias Schultze <http://tobion.de>[m
[32m+[m[32m */[m
[32m+[m[32mclass HelloWorldCommand extends Command[m
[32m+[m[32m{[m
[32m+[m[32m    /**[m
[32m+[m[32m     * {@inheritdoc}[m
[32m+[m[32m     */[m
[32m+[m[32m    protected function configure()[m
[32m+[m[32m    {[m
[32m+[m[32m        $this[m
[32m+[m[32m            ->setName('acme:hello')[m
[32m+[m[32m            ->setDescription('Hello World example command')[m
[32m+[m[32m            ->addArgument('who', InputArgument::OPTIONAL, 'Who to greet.', 'World')[m
[32m+[m[32m            ->setHelp(<<<EOF[m
[32m+[m[32mThe <info>%command.name%</info> command greets somebody or everybody:[m
[32m+[m
[32m+[m[32m<info>php %command.full_name%</info>[m
[32m+[m
[32m+[m[32mThe optional argument specifies who to greet:[m
[32m+[m
[32m+[m[32m<info>php %command.full_name%</info> Fabien[m
[32m+[m[32mEOF[m
[32m+[m[32m            );[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * {@inheritdoc}[m
[32m+[m[32m     */[m
[32m+[m[32m    protected function execute(InputInterface $input, OutputInterface $output)[m
[32m+[m[32m    {[m
[32m+[m[32m        $output->writeln(sprintf('Hello <comment>%s</comment>!', $input->getArgument('who')));[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[1mdiff --git a/src/Acme/DemoBundle/Controller/DemoController.php b/src/Acme/DemoBundle/Controller/DemoController.php[m
[1mnew file mode 100644[m
[1mindex 0000000..a99de89[m
[1m--- /dev/null[m
[1m+++ b/src/Acme/DemoBundle/Controller/DemoController.php[m
[36m@@ -0,0 +1,56 @@[m
[32m+[m[32m<?php[m
[32m+[m
[32m+[m[32mnamespace Acme\DemoBundle\Controller;[m
[32m+[m
[32m+[m[32muse Symfony\Bundle\FrameworkBundle\Controller\Controller;[m
[32m+[m[32muse Symfony\Component\HttpFoundation\RedirectResponse;[m
[32m+[m[32muse Symfony\Component\HttpFoundation\Request;[m
[32m+[m[32muse Acme\DemoBundle\Form\ContactType;[m
[32m+[m
[32m+[m[32m// these import the "@Route" and "@Template" annotations[m
[32m+[m[32muse Sensio\Bundle\FrameworkExtraBundle\Configuration\Route;[m
[32m+[m[32muse Sensio\Bundle\FrameworkExtraBundle\Configuration\Template;[m
[32m+[m
[32m+[m[32mclass DemoController extends Controller[m
[32m+[m[32m{[m
[32m+[m[32m    /**[m
[32m+[m[32m     * @Route("/", name="_demo")[m
[32m+[m[32m     * @Template()[m
[32m+[m[32m     */[m
[32m+[m[32m    public function indexAction()[m
[32m+[m[32m    {[m
[32m+[m[32m        return array();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * @Route("/hello/{name}", name="_demo_hello")[m
[32m+[m[32m     * @Template()[m
[32m+[m[32m     */[m
[32m+[m[32m    public function helloAction($name)[m
[32m+[m[32m    {[m
[32m+[m[32m        return array('name' => $name);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * @Route("/contact", name="_demo_contact")[m
[32m+[m[32m     * @Template()[m
[32m+[m[32m     */[m
[32m+[m[32m    public function contactAction(Request $request)[m
[32m+[m[32m    {[m
[32m+[m[32m        $form = $this->createForm(new ContactType());[m
[32m+[m[32m        $form->handleRequest($request);[m
[32m+[m
[32m+[m[32m        if ($form->isValid()) {[m
[32m+[m[32m            $mailer = $this->get('mailer');[m
[32m+[m
[32m+[m[32m            // .. setup a message and send it[m
[32m+[m[32m            // http://symfony.com/doc/current/cookbook/email.html[m
[32m+[m
[32m+[m[32m            $request->getSession()->getFlashBag()->set('notice', 'Message sent!');[m
[32m+[m
[32m+[m[32m            return new RedirectResponse($this->generateUrl('_demo'));[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return array('form' => $form->createView());[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
