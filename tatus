[32m+[m[32m<?php[m
[32m+[m
[32m+[m[32mnamespace Acme\DemoBundle\Tests\Controller;[m
[32m+[m
[32m+[m[32muse Symfony\Bundle\FrameworkBundle\Test\WebTestCase;[m
[32m+[m
[32m+[m[32mclass DemoControllerTest extends WebTestCase[m
[32m+[m[32m{[m
[32m+[m[32m    public function testIndex()[m
[32m+[m[32m    {[m
[32m+[m[32m        $client = static::createClient();[m
[32m+[m
[32m+[m[32m        $crawler = $client->request('GET', '/demo/hello/Fabien');[m
[32m+[m
[32m+[m[32m        $this->assertGreaterThan(0, $crawler->filter('html:contains("Hello Fabien")')->count());[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public function testSecureSection()[m
[32m+[m[32m    {[m
[32m+[m[32m        $client = static::createClient();[m
[32m+[m
[32m+[m[32m        // goes to the secure page[m
[32m+[m[32m        $crawler = $client->request('GET', '/demo/secured/hello/World');[m
[32m+[m
[32m+[m[32m        // redirects to the login page[m
[32m+[m[32m        $crawler = $client->followRedirect();[m
[32m+[m
[32m+[m[32m        // submits the login form[m
[32m+[m[32m        $form = $crawler->selectButton('Login')->form(array('_username' => 'admin', '_password' => 'adminpass'));[m
[32m+[m[32m        $client->submit($form);[m
[32m+[m
[32m+[m[32m        // redirect to the original page (but now authenticated)[m
[32m+[m[32m        $crawler = $client->followRedirect();[m
[32m+[m
[32m+[m[32m        // check that the page is the right one[m
[32m+[m[32m        $this->assertCount(1, $crawler->filter('h1.title:contains("Hello World!")'));[m
[32m+[m
[32m+[m[32m        // click on the secure link[m
[32m+[m[32m        $link = $crawler->selectLink('Hello resource secured')->link();[m
[32m+[m[32m        $crawler = $client->click($link);[m
[32m+[m
[32m+[m[32m        // check that the page is the right one[m
[32m+[m[32m        $this->assertCount(1, $crawler->filter('h1.title:contains("secured for Admins only!")'));[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[1mdiff --git a/src/Acme/DemoBundle/Twig/Extension/DemoExtension.php b/src/Acme/DemoBundle/Twig/Extension/DemoExtension.php[m
[1mnew file mode 100644[m
[1mindex 0000000..def0757[m
[1m--- /dev/null[m
[1m+++ b/src/Acme/DemoBundle/Twig/Extension/DemoExtension.php[m
[36m@@ -0,0 +1,81 @@[m
[32m+[m[32m<?php[m
[32m+[m
[32m+[m[32mnamespace Acme\DemoBundle\Twig\Extension;[m
[32m+[m
[32m+[m[32muse CG\Core\ClassUtils;[m
[32m+[m
[32m+[m[32mclass DemoExtension extends \Twig_Extension[m
[32m+[m[32m{[m
[32m+[m[32m    protected $loader;[m
[32m+[m[32m    protected $controller;[m
[32m+[m
[32m+[m[32m    public function __construct(\Twig_LoaderInterface $loader)[m
[32m+[m[32m    {[m
[32m+[m[32m        $this->loader = $loader;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public function setController($controller)[m
[32m+[m[32m    {[m
[32m+[m[32m        $this->controller = $controller;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * {@inheritdoc}[m
[32m+[m[32m     */[m
[32m+[m[32m    public function getFunctions()[m
[32m+[m[32m    {[m
[32m+[m[32m        return array([m
[32m+[m[32m            new \Twig_SimpleFunction('code', array($this, 'getCode'), array('is_safe' => array('html'))),[m
[32m+[m[32m        );[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public function getCode($template)[m
[32m+[m[32m    {[m
[32m+[m[32m        // highlight_string highlights php code only if '<?php' tag is present.[m
[32m+[m[32m        $controller = highlight_string("<?php" . $this->getControllerCode(), true);[m
[32m+[m[32m        $controller = str_replace('<span style="color: #0000BB">&lt;?php&nbsp;&nbsp;&nbsp;&nbsp;</span>', '&nbsp;&nbsp;&nbsp;&nbsp;', $controller);[m
[32m+[m
[32m+[m[32m        $template = htmlspecialchars($this->getTemplateCode($template), ENT_QUOTES, 'UTF-8');[m
[32m+[m
[32m+[m[32m        // remove the code block[m
[32m+[m[32m        $template = str_replace('{% set code = code(_self) %}', '', $template);[m
[32m+[m
[32m+[m[32m        return <<<EOF[m
[32m+[m[32m<p><strong>Controller Code</strong></p>[m
[32m+[m[32m<pre>$controller</pre>[m
[32m+[m
[32m+[m[32m<p><strong>Template Code</strong></p>[m
[32m+[m[32m<pre>$template</pre>[m
[32m+[m[32mEOF;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    protected function getControllerCode()[m
[32m+[m[32m    {[m
[32m+[m[32m        $class = get_class($this->controller[0]);[m
[32m+[m[32m        if (class_exists('CG\Core\ClassUtils')) {[m
[32m+[m[32m            $class = ClassUtils::getUserClass($class);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        $r = new \ReflectionClass($class);[m
[32m+[m[32m        $m = $r->getMethod($this->controller[1]);[m
[32m+[m
[32m+[m[32m        $code = file($r->getFilename());[m
[32m+[m
[32m+[m[32m        return '    '.$m->getDocComment()."\n".implode('', array_slice($code, $m->getStartline() - 1, $m->getEndLine() - $m->getStartline() + 1));[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    protected function getTemplateCode($template)[m
[32m+[m[32m    {[m
[32m+[m[32m        return $this->loader->getSource($template->getTemplateName());[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Returns the name of the extension.[m
[32m+[m[32m     *[m
[32m+[m[32m     * @return string The extension name[m
[32m+[m[32m     */[m
[32m+[m[32m    public function getName()[m
[32m+[m[32m    {[m
[32m+[m[32m        return 'demo';[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
